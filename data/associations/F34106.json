                        {
                          "type": "text",
                          "text": "Une association doit payer la taxe d'habitation sur ses "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "locaux meublés occupés pour son administration générale à titre privatif"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " sauf s'ils relèvent de la "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F23547",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Professionnels"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "cotisation foncière des entreprises (CFE)"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Il en est de même pour les "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "locaux mis à sa disposition gratuitement"
