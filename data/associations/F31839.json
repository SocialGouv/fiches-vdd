          ]
        },
        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R2046"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Bénévole"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Personne qui s'engage librement pour mener une action non salariée en direction d'autrui, en dehors de son temps professionnel et familial"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R52183"
          },
          "children": [
            {
              "type": "element",
