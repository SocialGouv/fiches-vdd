                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Dons effectués par un particulier"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "BlocCas",
                      "attributes": {
                        "affichage": "radio"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Organisme d'intérêt général ou reconnu d'utilité publique"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "La réduction d’impôt  est de "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "66 %"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " du montant des dons. La réduction s'applique  dans la limite de "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "20 %"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " du revenu imposable."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Organisme d'aide aux personnes en difficulté fournissant gratuitement des repas, des soins ou favorisant le logement"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Pour la partie du don  inférieure ou égale à "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "537 €"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": ", la réduction d'impôt est de "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "75 %"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " du montant donné."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Pour la partie du don supérieure à "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "537 €"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": ", le montant de la réduction d'impôt  est de "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "66 %"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " du montant donné."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Le montant cumulé des dons qui donnent droit à la réduction d'impôt ne peut  pas dépasser "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "20 %"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " de votre revenu imposable."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Organisme destiné au relogement des victimes de violence domestique"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Les dons doivent être versés à partir de 1"
                                },
                                {
                                  "type": "element",
                                  "name": "Exposant",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "er"
                                    }
                                  ]
                                },
