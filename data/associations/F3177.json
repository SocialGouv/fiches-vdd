          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les dons en nature accordés notamment aux "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F34246",
                    "type": "Fiche Question-réponse",
                    "audience": "Associations"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "organismes d’intérêt général"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ouvrent droit à une réduction d'impôt dans les conditions suivantes :"
                }
              ]
            },
            {
              "type": "element",
              "name": "BlocCas",
              "attributes": {
                "affichage": "onglet"
