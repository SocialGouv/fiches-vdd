                {
                  "type": "element",
                  "name": "Variable",
                  "children": [
                    {
                      "type": "text",
                      "text": "date de la déclaration"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", a vocation à recevoir des subventions publiques."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "Variable",
                  "children": [
                    {
                      "type": "text",
                      "text": "Selon la situation :"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " Notre association comprend le ou les établissements suivants : "
                },
                {
                  "type": "element",
                  "name": "Variable",
                  "children": [
                    {
                      "type": "text",
                      "text": "nom"
                    }
                  ]
