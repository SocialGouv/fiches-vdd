                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Ancienneté du salarié"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Montant de l'indemnité"
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Rangée",
                      "attributes": {
                        "type": "normal"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Inférieure à 1 an"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "2 mois de salaire"
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Rangée",
                      "attributes": {
                        "type": "normal"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Entre 1 an et moins de 8 ans"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "3 mois de salaire pour 1 an, auxquels s'ajoutent 1 mois supplémentaire par année d'ancienneté"
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Rangée",
                      "attributes": {
                        "type": "normal"
