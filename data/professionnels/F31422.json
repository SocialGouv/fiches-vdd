                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Cas général"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Cette attestation est délivrée par l'Urssaf sur votre compte :"
                            }
                          ]
                        },
