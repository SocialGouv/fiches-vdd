                  "children": [
                    {
                      "type": "text",
                      "text": "Arbitrage"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Theme",
                  "attributes": {
                    "ID": "N24266"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
