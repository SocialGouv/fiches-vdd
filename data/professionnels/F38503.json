                          "children": [
                            {
                              "type": "text",
                              "text": "À noter"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "60 000 rebonds fait partie des associations partenaires du "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
