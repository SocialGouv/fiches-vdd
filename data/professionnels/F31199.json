                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "8 €"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " par objet en or ou en platine ("
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "2 €"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " en outre-mer) ;"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "4 €"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " par objet en argent ("
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "1 €"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " en outre-mer)."
                                }
                              ]
                            }
                          ]
