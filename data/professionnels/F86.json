        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Qu'est-ce que le solde de tout compte ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le reçu pour solde de tout compte fait partie des "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F21789",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Professionnels"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "documents"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " que votre employeur doit vous remettre à la fin de votre contrat de travail."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le reçu pour solde de tout compte fait l'inventaire de l'ensemble des sommes qui vous sont versées."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Votre solde de tout compte vous est versé par chèque ou par virement bancaire à la fin de votre contrat de travail."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Que doit comporter le solde de tout compte ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les mentions devant figurer sur votre solde de tout compte sont les suivantes :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Total des sommes qui vous sont dues à la date de fin de votre contrat (salaire, primes, indemnités de rupture du contrat)"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Mention selon laquelle le reçu pour solde de tout compte est établi en double exemplaire (dont l'un vous est remis)"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quand votre solde de tout compte vous est-il remis ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Cela dépend si vous effectuez un préavis (c'est-à-dire une période de travail entre l'annonce de votre fin de contrat de travail et la fin réelle du contrat) ou non."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "BlocCas",
                  "attributes": {
                    "affichage": "radio"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous effectuez un préavis"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Votre employeur vous remet l'un des 2 exemplaires du reçu pour solde de tout compte à la fin de votre préavis."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Votre employeur doit vous informer que le document est prêt et vous le remettre."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "ANoter",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "À noter"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "la loi ne vous oblige pas à signer le reçu pour solde de tout compte."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous êtes dispensé d'effectuer un préavis"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Votre employeur vous remet l'un des 2 exemplaires du reçu pour solde de tout compte."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "En cas de dispense du préavis, votre employeur peut vous remettre le reçu le jour de votre départ physique de l'entreprise, sans attendre la fin du préavis initialement prévu."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "ANoter",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Exemple"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous êtes dispensé d'effectuer votre préavis qui devait avoir lieu du 1"
                                },
                                {
                                  "type": "element",
                                  "name": "Exposant",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "er"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " au 31 octobre. Votre employeur peut vous remettre le reçu le 30 septembre."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Votre employeur doit vous informer que le document est prêt et vous le remettre."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "ANoter",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "À noter"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "la loi ne vous oblige pas à signer le reçu pour solde de tout compte."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Peut-on contester les sommes mentionnées sur le solde de tout compte ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous pouvez contester les sommes indiquées sur le solde de tout compte si vous estimez que votre employeur a (intentionnellement ou non) fait une erreur ou un oubli en calculant les différentes rémunérations ou indemnités."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le délai pour contester ces sommes dépend de votre signature ou de l'absence de votre signature du reçu."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "BlocCas",
                  "attributes": {
                    "affichage": "radio"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous avez signé le reçu"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous avez "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "6 mois"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " après la date de votre signature pour contester le reçu pour solde de tout compte."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "La contestation doit être adressée à l'employeur par lettre recommandée."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Votre courrier précise alors les motifs de la contestation et les sommes concernées (arriéré de salaire ou indemnités de congés payés, par exemple)."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Votre employeur n'est pas obligé de mentionner le délai de contestation de 6 mois sur le solde de tout compte."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Si le recours porte sur des sommes qui ne sont pas mentionnées dans le reçu, vous avez un délai de "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "3 ans"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " pour en réclamer le paiement."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Attention",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Attention"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Votre employeur peut vous réclamer les sommes versées en trop pendant "
                                },
                                {
                                  "type": "element",
                                  "name": "MiseEnEvidence",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "3 ans"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous n'avez pas signé le reçu"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
