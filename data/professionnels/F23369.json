                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Activité commerciale, y compris hébergement et restauration"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
