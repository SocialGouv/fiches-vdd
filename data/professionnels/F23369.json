                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Rangée",
                      "attributes": {
                        "type": "normal"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Prestation de services commerciale ou artisanale (BIC)"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "50 %"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Rangée",
                      "attributes": {
                        "type": "normal"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Prestation de services commerciale ou artisanale et professions libérales non réglementées"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "(BNC)"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cellule",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
