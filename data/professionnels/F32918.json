                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "ensemble des rémunérations :"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Liste",
                                  "attributes": {
                                    "type": "puce"
                                  },
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "element",
                                              "name": "Valeur",
                                              "children": [
                                                {
                                                  "type": "text",
                                                  "text": "300 000 €"
                                                }
                                              ]
                                            },
                                            {
                                              "type": "text",
                                              "text": " (pour 10 personnes)"
                                            }
                                          ]
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "ou "
                                            },
                                            {
                                              "type": "element",
                                              "name": "Valeur",
                                              "children": [
                                                {
                                                  "type": "text",
                                                  "text": "150 000 €"
                                                }
                                              ]
                                            },
                                            {
                                              "type": "text",
                                              "text": " (pour 5 personnes)"
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  ]
                                }
                              ]
                            },
