                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "R45943",
                    "type": "Sigle"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "PTAC"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " du véhicule ou de la remorque."
                }
