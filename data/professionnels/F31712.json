        },
        {
          "type": "element",
          "name": "Introduction",
          "children": [
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Une société créée uniquement pour le rachat d'une entreprise peut bénéficier d'un crédit d'impôt sous certaines conditions."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Conditions d'octroi du crédit d'impôt"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le crédit d'impôt s'applique si :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "l'entreprise rachetée et la société créée sont assujetties à l'impôt sur les sociétés (IS) et ne font pas partie du même groupe d'intégration fiscale ;"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "le capital de la société nouvelle est détenu :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "par au moins 15  salariés de la société reprise ;"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "ou par au moins "
                                    },
                                    {
                                      "type": "element",
                                      "name": "Valeur",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "30 %"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "text",
                                      "text": " des salariés de cette société si l'effectif ne dépasse pas 50 salariés ;"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "la reprise  fait l'objet d'un accord d'entreprise."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Pour bénéficier du crédit d'impôt, l'entreprise concernée ne doit pas déjà bénéficier d'autres exonérations d'impôts (jeune entreprise innovante-JEI, par exemple)."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ANoter",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "À noter"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "à partir du "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "1"
                            },
                            {
                              "type": "element",
                              "name": "Exposant",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "er"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " janvier 2020"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", la condition de détention par un nombre minimum de salariés de la société rachetée des droits de vote de la nouvelle entreprise est supprimée. Cette mesure s'appliquera jusqu'au "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "31 décembre 2022"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Montant du crédit d'impôt"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le crédit d'impôt est égal, pour chaque exercice, au montant de l'impôt sur les sociétés (IS) dû par la société reprise, dans la proportion du capital détenu par les salariés de la société rachetée et dans la limite  des intérêts d'emprunts dus par la société nouvelle."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "L'impôt sur les sociétés dû correspond au montant dû avant imputation des réductions et crédits d'impôt."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les intérêts  pris en compte doivent être ceux qui viennent à échéance au cours de l'exercice au titre duquel le crédit d'impôt est calculé."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le crédit d'impôt doit être imputé sur l'impôt sur les bénéfices dû par l'entreprise au titre de l'année au cours de laquelle les dépenses éligibles ont été engagées, après les prélèvements obligatoires et les autres crédits d'impôt."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Lorsque le montant de l'impôt est insuffisant pour imputer la totalité du crédit d'impôt, l'excédent peut être restitué à l'entreprise au moyen d'une demande de remboursement de crédits d'impôt "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "R19866",
                        "type": "Formulaire",
                        "audience": "Professionnels"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "(imprimé n°2573-SD)"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Comment demander le crédit d'impôt ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La société assujettie à l'impôt sur les sociétés doit déposer une déclaration spéciale "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "R39692",
                        "type": "Formulaire",
                        "audience": "Professionnels"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "n°2069-RCI-SD"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " pour bénéficier d'un crédit d'impôt."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La fiche d'aide au calcul"
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "R20111",
                        "type": "Formulaire",
                        "audience": "Professionnels"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": " n°2079-RS-FC-SD"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " facilite la détermination du montant du crédit d'impôt. Cette fiche d'aide au calcul peut être déposée auprès de l'administration fiscale."
                    }
                  ]
                }
              ]
            }
          ]
        },
