        {
          "type": "element",
          "name": "Avertissement",
          "attributes": {
            "ID": "R72666",
            "date": "2025-07-01"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Téléprocédure TVA et taxes annexes : accès aux nouveaux formulaires CA3 de déclaration retardé au 7 juillet 2025"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "En raison d’un "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "problème technique"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", la DGFIP a indiqué, dans un "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.impots.gouv.fr/actualite/declarer-tva-mise-disposition-de-la-nouvelle-version-du-formulaire-compter-du-7-juillet"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "communiqué en date du 1er juillet 2025"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", que le nouveau formulaire CA3 de déclaration de TVA et son annexe ne seront disponibles qu’à compter du "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "7 juillet 2025"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " (au lieu du 1"
                    },
                    {
                      "type": "element",
                      "name": "Exposant",
                      "children": [
                        {
                          "type": "text",
                          "text": "er"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " juillet 2025)."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les déclarations au titre du mois de "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "juin"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " devront être transmises entre le 15 et le 24 juillet conformément au calendrier fiscal."
                    }
                  ]
                }
              ]
            }
