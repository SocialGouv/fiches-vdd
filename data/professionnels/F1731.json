        {
          "type": "element",
          "name": "Introduction",
          "children": [
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "interdit"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " de laisser les salariés prendre leur repas dans les "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "locaux affectés au travail"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". L'employeur doit mettre à disposition des salariés un "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "local"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " de restauration ou un "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "emplacement"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " pour se restaurer. Nous faisons le point sur la réglementation, qui "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "diffère"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " selon "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "l'effectif"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " de l'entreprise."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "ListeSituations",
          "attributes": {
            "affichage": "onglet"
          },
          "children": [
            {
              "type": "element",
              "name": "Situation",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Moins de 50 salariés"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Introduction",
                  "children": [
                    {
                      "type": "element",
                      "name": "Texte",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "L'employeur doit mettre en place"
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": " un emplacement "
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "pour permettre aux salariés de "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "se restaurer"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " dans de bonnes conditions de santé et de sécurité."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "L'emplacement pour se restaurer peut-il se situer dans les locaux de travail ?"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Lorsque l'activité de ces locaux de travail"
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": " ne comporte pas"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " l'emploi ou le stockage de substances ou de mélanges dangereux, l'employeur peut, "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "après déclaration à l'inspection du travail et à la médecine du travail"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ", choisir ces locaux de travail comme emplacement pour se restaurer."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "L'employeur doit adresser une déclaration comportant les informations suivantes :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Identité de l'employeur ou raison sociale de l'entreprise ou de l'établissement"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Secteur d'activité de l'entreprise ou de l'établissement"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Adresse du site concerné et le numéro Siret de l'établissement"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Nombre de travailleurs concernés"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Caractéristiques des locaux affectés au travail et emplacement permettant aux salariés de se restaurer dans de bonnes conditions de santé et de sécurité."
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Cette déclaration doit être transmise avant l'aménagement de l'emplacement réservé à la restauration dans les locaux de travail."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Elle est datée et est effectuée par tout moyen."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "L'emplacement pour se restaurer doit-il être aménagé ?"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Cet emplacement "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "n'est pas obligatoirement"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " équipé d'un moyen de conservation ou de réfrigération des aliments et des boissons, ni d'une installation permettant de réchauffer les plats."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "L'installation d'un robinet d'eau potable fraîche ou chaude "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "n'est pas obligatoire"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "L'employeur n'est pas obligé d'y disposer chaises et tables en nombre suffisant."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Qui entretient l'emplacement permettant de se restaurer ?"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Après chaque repas, l'employeur veille "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "au nettoyage de cet emplacement et des équipements qui y sont installés"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Situation",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "50 salariés et plus"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Introduction",
                  "children": [
                    {
                      "type": "element",
                      "name": "Texte",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Dans les entreprises de 50 salariés et plus, l'employeur doit mettre en place un "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "local de restauration"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ". Il doit "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "consulter"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " le "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "comité social et économique (CSE)"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " avant la mise en place de ce local."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Le local de restauration doit-il être aménagé ?"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "L'employeur "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "doit"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " mettre à disposition un "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "local de restauration aménagé"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Ce local contient les équipements suivants :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
