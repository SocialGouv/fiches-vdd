        {
          "type": "element",
          "name": "Introduction",
          "children": [
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les exonérations de CFE peuvent être permanentes ou temporaires."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "ASavoir",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À savoir"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "depuis le "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "1"
                        },
                        {
                          "type": "element",
                          "name": "Exposant",
                          "children": [
                            {
                              "type": "text",
                              "text": "er"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " janvier 2019"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", les syndicats professionnels ne sont plus exonérés de CFE pour leurs activités portant sur l'étude et la défense des droits et intérêts de leurs membres."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Certaines entreprises sont exonérées de CFE de manière permanente, sous réserve de respecter certaines conditions :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les collectivités territoriales, les établissements publics et les organismes de l'État ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les grands ports maritimes, les ports autonomes, ainsi que les ports gérés par des collectivités territoriales, des établissements publics ou des sociétés d'économie mixte (à l'exception des ports de plaisance) ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les exploitants agricoles, certains groupements d'employeurs et certains "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "R24404",
                            "type": "Sigle"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "GIE"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "certaines coopératives agricoles et leurs unions ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les artisans * travaillant soit à façon pour les particuliers, soit pour leur compte avec des matières leur appartenant, qu'ils aient ou non une enseigne ou une boutique, lorsqu'ils utilisent uniquement le concours d'apprenti(s) âgé(s) de 20 ans maximum au début de l'apprentissage ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les chauffeurs de taxis ou d'ambulances,\n\t\tpropriétaires ou locataires d'1 ou 2 voitures, de 7 places maximum (sans compter la place du chauffeur), qu'ils conduisent ou gèrent eux-mêmes, à condition que les 2 voitures ne soient pas en service en même temps et qu'ils  respectent les "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F22127",
                            "type": "Fiche d'information",
                            "audience": "Professionnels"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "tarifs réglementaires ;"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les coopératives et unions de sociétés coopératives d'artisans, de même que les sociétés coopératives et unions de sociétés coopératives de patrons bateliers, les sociétés coopératives maritimes ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "certains pêcheurs, sociétés de pêche artisanale et inscrits maritimes ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les coopératives ouvrières de production (Scop) ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les vendeurs à domicile indépendants (VDI), pour leur  rémunération brute totale inférieure à "
                        },
                        {
                          "type": "element",
                          "name": "Valeur",
                          "children": [
                            {
                              "type": "text",
                              "text": "6 686 €"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les établissements privés d'enseignement du premier et second degré sous contrat avec l'État et d'enseignement supérieur sous convention  ou reconnus d'utilité publique ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les professeurs de lettres, sciences et arts d'agrément et instituteurs primaires qui dispensent leur enseignement personnellement, soit à leur domicile ou au domicile de leurs élèves, soit dans un local dépourvu d'enseigne et ne comportant pas un aménagement spécial ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les peintres, sculpteurs, graveurs et dessinateurs considérés comme artistes et ne vendant que le produit de leur art ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les photographes auteurs, pour leur activité relative à la réalisation de prises de vues et à la cession de leurs œuvres d'art ou de droits d'auteur ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les auteurs, compositeurs, chorégraphes, traducteurs percevant des droits d'auteur, et certaines catégories d'entrepreneurs de spectacles (spectacles vivants) ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les artistes lyriques et dramatiques ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les disquaires indépendants (avec pour activité principale la vente au détail de phonogrammes) ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les établissements de vente de livres neufs au détail possédant le label de librairie indépendante de référence ou, pour ceux n'ayant pas le label ,  disposant d'un local librement accessible au public et une activité de vente de livres neufs au détail représentant au moins "
                        },
                        {
                          "type": "element",
                          "name": "Valeur",
                          "children": [
                            {
                              "type": "text",
                              "text": "50 %"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " du chiffre d'affaires ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les sages-femmes et garde-malades, (sauf s'ils relèvent de la profession d'infirmière) ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les médecins et auxiliaires de santé ouvrant un cabinet secondaire dans un désert médical ou dans une commune de moins de 2000 habitants ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les sportifs pour la seule pratique d'un sport ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "certaines activités de presse, comme les diffuseurs de presse spécialistes, exonérés de plein droit"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les organismes HLM et les propriétaires ou locataires louant ou sous-louant une partie de leur habitation personnelle de façon occasionnelle à un prix raisonnable (ou en meublé, à condition que le logement constitue la résidence principale du sous-locataire) ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les exploitants de meublé de tourisme classé ou de chambre d'hôtes (sauf délibération contraire de la commune), à condition que ces locaux fassent partie de leur habitation personnelle (résidence principale ou secondaire en dehors des périodes de location) et ne constituent pas l'habitation principale ou secondaire du locataire ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les entreprises pour leur activité de production de biogaz, d'électricité et de chaleur par la méthanisation ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les activités à caractère social (sauf les mutuelles, leurs unions, et les institutions de prévoyance) ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les caisses de crédit municipal."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les entreprises doivent demander l'exonération de la CFE en utilisant le formulaire cerfa n°10694*20."
                }
              ]
            },
            {
              "type": "element",
              "name": "ServiceEnLigne",
              "attributes": {
                "ID": "R14646",
                "URL": "https://www.impots.gouv.fr/portail/node/8832",
                "numerocerfa": "10694*19",
                "autrenumero": "1465-SD",
                "type": "Formulaire"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Exonération temporaire de cotisation foncière des entreprises (CFE)"
                    }
                  ]
                },
