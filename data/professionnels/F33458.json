            },
            {
              "type": "element",
              "name": "TitreFlottant",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "À qui et comment transmettre la fiche individuelle de police ?"
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le loueur doit conserver la fiche individuelle de police pendant"
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": " 6 mois"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "transmission"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " de la fiche à la police ou à la gendarmerie n'est "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "pas systématique"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Le loueur doit la transmettre seulement si les services de police ou de gendarmerie l'exigent."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les services de police ou de gendarmerie peuvent demander la fiche du touriste étranger "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "uniquement"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " pour l'un des 3 "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "motifs"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " suivants :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
