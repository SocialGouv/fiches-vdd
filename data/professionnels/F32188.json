        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R66113",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "produits préemballés"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", qu'ils soient alimentaires ou non, doivent comporter une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "inscription"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " permettant d'identifier l'un des professionnels suivants :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Conditionneur ou emplisseur (celui qui réalise le préemballage)"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Fabricant du produit (celui pour qui le préemballage est effectué)"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Importateur (établi dans l'"
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R41270",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Union européenne"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ")."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Cette identification présente sur l'emballage se fait, au choix du professionnel : soit "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "en clair"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " (nom et adresse du professionnel en toutes lettres), soit "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "en code"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Dans ce second cas, on parle de « "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "element",
                      "name": "Expression",
                      "children": [
                        {
                          "type": "text",
                          "text": "code emballeur EMB"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " »."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Comment se présente le code emballeur EMB ?"
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le code emballeur prend la forme de "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "5 chiffres"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " (correspondant au code géographique de la commune et du département d'où provient le produit), précédés de la "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "mention « "
                    },
                    {
                      "type": "element",
                      "name": "Expression",
                      "children": [
                        {
                          "type": "text",
                          "text": "EMB"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " »"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". S'il y a au moins 2 professionnels qui exercent la même activité dans la commune, le code est complété par une ou plusieurs lettres complémentaires pour identifier chaque entreprise."
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Exemple"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "« EMB 29004G » pour un emplisseur situé à Bannalec dans le Finistère."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Toutefois, le code emballeur EMB est "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "différent"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " de l'"
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "agrément sanitaire européen"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " (ou estampille sanitaire) destiné à tout établissement qui prépare, traite, transforme, manipule ou entrepose des denrées animales ou d'origine animale (fromages, charcuterie, produits de la mer, etc.)."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le code emballeur et l'estampille sanitaire (pictogramme ovale) sont des "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "codes de traçabilité"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Ils peuvent figurer "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "tous les 2 sur un même emballage"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L'agrément sanitaire est composé quant à lui de la manière suivante :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Pays d'origine du produit"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Numéro du département"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Numéro de la commune"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Numéro attribué à l'entreprise de production"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Sigle « CE » indiquant l’origine Européenne du produit."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Exemple"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "« FR 85.086.022 CE » pour un producteur situé à Falleron en Vendée."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À noter"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Une "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://fr.openfoodfacts.org/codes-emballeurs"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "liste des codes de traçabilité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " en France est mise à disposition."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "À qui s'adresser pour se procurer un code emballeur EMB ?"
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Pour obtenir un code emballeur (EMB), vous devez faire une demande écrite, sur papier libre, à la "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "direction départementale de la protection des populations (DDPP)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "OuSAdresser",
              "attributes": {
                "ID": "R18",
                "sve": "non",
                "type": "Local personnalisable"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Direction départementale de la protection des populations (DDPP)"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "PivotLocal",
                  "children": [
                    {
                      "type": "text",
                      "text": "ddpp"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "RessourceWeb",
                  "attributes": {
                    "URL": "https://www.economie.gouv.fr/dgccrf/coordonnees-des-DDPP-et-DDETSPP"
                  }
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Dans votre courrier, vous devez préciser les "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "informations suivantes"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Nom ou raison sociale"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F33050",
                            "type": "Fiche Question-réponse conditionnée",
                            "audience": "Professionnels"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Code APE"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F21000",
                            "type": "Fiche Question-réponse conditionnée",
                            "audience": "Professionnels"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Extrait Kbis"
                            }
                          ]
                        }
                      ]
                    }
