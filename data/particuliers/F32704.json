                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "De votre situation (salarié, demandeur d'emploi...)"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Et de votre lieu d'habitation."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
