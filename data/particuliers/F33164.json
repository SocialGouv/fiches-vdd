                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "L'assistante maternelle a droit à une "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "indemnité compensatrice"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", calculée comme l'indemnité représentative de congés, qui correspond aux congés payés dus et non pris au titre de "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R51495",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "l'année de référence"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " et de l'année en cours."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Cette indemnité est égale à 1/10"
                    },
                    {
                      "type": "element",
                      "name": "Exposant",
                      "children": [
                        {
                          "type": "text",
                          "text": "ème"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " de la rémunération brute totale perçue par le salarié au cours de "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R51495",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "l'année de référence"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "  (indemnité de congés payés de l'année précédente incluse)."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ANoter",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Exemple"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Début du contrat le 1er septembre 2019, retrait d'enfant le 31 août 2020 (fin de préavis)."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'assistante maternelle perçoit 755€ bruts par mois."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'indemnité compensatrice est égale au dixième des rémunérations brutes totales perçues entre le début et la fin du contrat (soit 755x12/10=906€)."
                        }
                      ]
                    }
                  ]
                }
