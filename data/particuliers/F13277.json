              "attributes": {
                "URL": "https://lannuaire.service-public.fr/navigation/mairie"
              }
            }
          ]
        },
        {
          "type": "element",
          "name": "Reference",
          "attributes": {
            "type": "Texte de référence",
            "URL": "https://www.legifrance.gouv.fr/codes/section_lc/LEGITEXT000006070239/LEGISCTA000006164051/",
            "ID": "R340"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
