                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Somme d'argent destinée à réparer le préjudice subi"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F36444",
