                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Correspond à la totalité des jours du calendrier de l'année civile, du 1"
                    },
                    {
                      "type": "element",
                      "name": "Exposant",
                      "children": [
                        {
                          "type": "text",
                          "text": "er"
                        }
                      ]
                    },
