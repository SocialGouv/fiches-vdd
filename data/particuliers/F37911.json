        {
          "type": "element",
          "name": "QuiPeutMAider",
          "attributes": {
            "ID": "R59248",
            "sve": "non",
            "type": "National"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "0809 540 550 DGCCRF - RéponseConso"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous rencontrez une difficulté suite à un achat ? Vous avez une interrogation sur un point de droit avant d'acheter ou commander ?"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous pouvez obtenir une réponse par un agent de la "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "R31769",
                        "type": "Sigle"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "DGCCRF"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " en appelant le "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "0809 540 550"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Horaires d'ouverture du service :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Lundi et mardi : de 8h30 à 12h30 et de 13h15 à 17h15"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Mercredi : de 13h15 à 17h15"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Jeudi : de 8h30 à 12h30"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vendredi : de 8h30 à 16h"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
