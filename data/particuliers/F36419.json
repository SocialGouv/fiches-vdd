                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F18785",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "responsabilité parentale"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " si l'enfant vit en France."
                }
