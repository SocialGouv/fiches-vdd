                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Consultation du répertoire national des certifications professionnelles (RNCP)"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Source",
                      "attributes": {
                        "ID": "R30794"
                      },
                      "children": [
                        {
                          "type": "text",
