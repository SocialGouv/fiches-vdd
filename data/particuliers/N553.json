            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Covid-19 : conditions d'exercice de la chasse pendant le confinement"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Un "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.legifrance.gouv.fr/jorf/id/JORFTEXT000042475143"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "décret du 29 octobre 2020"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n interdit "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "à partir du 30 octobre 2020"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " tout déplacement hors de son domicile  "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.gouvernement.fr/info-coronavirus"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "sauf dérogations"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les activités de régulation de la faune sauvage et des espèces animales susceptibles d'occasionner des dégâts font partie des dérogations sous le contrôle des préfets de département."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les autres activités de chasse sont interdites."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "C'est ce que la secrétaire d'État auprès de la ministre de la transition écologique a précisé par "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.ecologie.gouv.fr/berangere-abba-precise-conditions-dexercice-chasse-pendant-confinement"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "communiqué de presse du 2 novembre 2020"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n."
                    }
                  ]
