          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si vous partez à la retraite "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "au-delà de l’âge de départ minimum"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " en ayant un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "nombre de trimestres d’assurance retraite supérieur"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " au nombre de trimestres exigé pour avoir droit à une "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F1781",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "retraite à taux plein"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", votre pension de retraite est majorée en fonction du nombre de trimestres supplémentaires que vous avez."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L’âge de départ minimum pris en compte est l"
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "’âge de départ minimum applicable aux fonctionnaires de "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R51645",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "catégorie sédentaire"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R51645",
                    "type": "Définition de glossaire"
                  }
                },
                {
                  "type": "text",
                  "text": " même si vous êtes fonctionnaire de "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R51644",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "catégorie active"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les trimestres travaillés supplémentaires pris en compte sont les "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R58586",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "trimestres civils"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " entiers suivant celui au cours duquel vous avez atteint l’âge minimum de départ en retraite."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Chaque trimestre civil entier travaillé supplémentaire augmente votre retraite de "
                },
                {
                  "type": "element",
                  "name": "Valeur",
                  "children": [
                    {
                      "type": "text",
                      "text": "1,25 %"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La surcote ne se cumule pas avec la "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F37311",
                    "type": "Fiche Question-réponse conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "surcote parentale"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " (accordée si vous avez atteint, à 63 ans, le nombre de trimestres exigé pour avoir droit à une retraite à taux plein et si vous bénéficiez d'au moins 1 trimestre d'assurance retraite gratuit pour enfant)."
                }
              ]
            },
            {
              "type": "element",
              "name": "Attention",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Attention"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si vous êtes "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "contractuel"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", vous pouvez bénéficier d'une surcote dans les mêmes conditions qu'un "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F19643",
                        "type": "Fiche Question-réponse conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "salarié du secteur privé"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
