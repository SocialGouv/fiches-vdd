                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F18074",
                        "type": "Fiche d'information",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "l'aide juridictionnelle"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " pour financer ces dépenses."
                    }
                  ]
                }
              ]
