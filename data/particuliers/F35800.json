                {
                  "type": "element",
                  "name": "Introduction",
                  "children": [
                    {
                      "type": "element",
                      "name": "Texte",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Dans une procédure par consentement mutuel, une convention de divorce doit être rédigée par des avocats. Une fois finalisée, la convention doit être enregistrée par un notaire ou bien "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R55691",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "homologuée"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " par le juge si un enfant des époux veut être auditionné."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "BlocCas",
                      "attributes": {
                        "affichage": "onglet"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Cas général"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Chacun époux "
                                },
                                {
                                  "type": "element",
                                  "name": "MiseEnEvidence",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "doit"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " prendre son avocat dans un "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F10567",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "divorce par consentement mutuel"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Un seul avocat ne peut pas représenter les deux époux."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "La convention de divorce doit être signée par les époux et "
                                },
                                {
                                  "type": "element",
                                  "name": "MiseEnEvidence",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "les 2 avocats"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " avant d'être soumise au notaire pour enregistrement."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Un enfant des époux souhaite être auditionné"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "La convention de "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F10567",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "divorce par consentement mutuel"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " doit être soumise au juge pour être homologuée."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Les époux peuvent prendre "
                                },
                                {
                                  "type": "element",
                                  "name": "MiseEnEvidence",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "un seul"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " avocat pour les représenter. Ils peuvent également prendre chacun leur avocat."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Un époux est sous mesure de protection (tutelle, curatelle, ...)"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Le divorce par consentement mutuel n'est pas possible. Les époux doivent "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F35837",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "divorcer devant le juge aux affaires familiales"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
