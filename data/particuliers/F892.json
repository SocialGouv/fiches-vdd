                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Écrit permettant de saisir un tribunal"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R52096"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
