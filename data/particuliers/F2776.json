                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Cellule",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Au moins 50 salariés et moins de 300 salariés"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Cellule",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "3 trimestres consécutifs"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Rangée",
                          "attributes": {
                            "type": "normal"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Cellule",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Au moins 300 salariés"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Cellule",
                              "children": [
                                {
