                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quand est délivré le carnet de santé maternité ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le carnet de santé maternité vous est délivré "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "gratuitement"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " par les services de votre département lors du "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F963",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "1er examen prénatal"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " obligatoire, c'est-à-dire avant la fin de votre 3"
                    },
                    {
                      "type": "element",
                      "name": "Exposant",
                      "children": [
                        {
                          "type": "text",
                          "text": "e"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " mois de grossesse."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Qui peut consulter le carnet de santé maternité ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Lors de chaque examen médical, le médecin ou la sage-femme consigne ses constatations et indications sur votre carnet de maternité."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le carnet est votre propriété"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". Il est strictement confidentiel et couvert par le secret médical."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les professionnels qui le consultent et qui y inscrivent des annotations sont soumis au secret professionnel."
                    }
                  ]
                }
              ]
            }
