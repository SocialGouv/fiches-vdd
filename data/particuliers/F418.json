                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les services des impôts peuvent contrôler votre "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F80",
