                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F110",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "réclamation"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ou en exerçant un "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F32477",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "recours amiable"
                            }
                          ]
