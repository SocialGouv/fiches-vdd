                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Préfet"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Maire"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Président de l'EPCI"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Administrateur provisoire s'il en existe un"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "1 ou plusieurs copropriétaire(s)"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le tribunal peut prononcer "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F762",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "l'état de carence de la copropriété et engager une procédure d'expropriation"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
