          "type": "element",
          "name": "Reference",
          "attributes": {
            "type": "Texte de référence",
            "URL": "http://www.legifrance.gouv.fr/affichCode.do?idSectionTA=LEGISCTA000006167418&cidTexte=LEGITEXT000006071154",
