            }
          ]
        },
        {
          "type": "element",
          "name": "Reference",
          "attributes": {
            "type": "Texte de référence",
            "URL": "https://www.legifrance.gouv.fr/codes/section_lc/LEGITEXT000025024948/LEGISCTA000025026032",
            "ID": "R36229"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Code des procédures civiles d'exécution : articles L412-1 à L412-8"
                }
              ]
            },
