                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Barrière de protection"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Elle doit respecter la norme NF P90-306."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Système d'alarme"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Il doit respecter la norme NF P90-307."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Couverture de sécurité"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Elle doit respecter la norme NF P90-308."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Abri"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Il doit respecter la norme NF P90-309."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
