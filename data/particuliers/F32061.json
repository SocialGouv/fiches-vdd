                      "attributes": {
                        "LienPublication": "F2665",
                        "type": "Fiche d'information",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "carnet d'entretien de l'immeuble"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " et le "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
