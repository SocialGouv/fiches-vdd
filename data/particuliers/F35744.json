        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Qu'est-ce que le passe sanitaire ?"
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "passe sanitaire"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " est une preuve sanitaire ou papier attestant de l'une des situations suivantes :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous disposez d'un "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F35611",
                                "type": "Fiche Question-réponse conditionnée",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "schéma complet de vaccination"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " et du délai nécessaire après la dernière injection"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous avez réalisé un "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "test antigénique ou PCR négatif de moins de 24 heures"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous avez les résultats d'un "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "test antigénique ou PCR positif"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " attestant du "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "element",
                              "name": "LienExterne",
                              "attributes": {
                                "URL": "https://www.gouvernement.fr/comment-obtenir-son-certificat-de-retablissement"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "rétablissement du Covid-19"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " datant de plus de 11 jours et de moins de 4 mois"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous avez un "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "certificat médical"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " attestant d'une contre-indication médicale à la vaccination"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Établissements dans lesquels le passe sanitaire reste exigé"
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous devez avoir un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "passe sanitaire"
