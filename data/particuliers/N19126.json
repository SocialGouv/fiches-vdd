          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Permis à points pour le titulaire d'un permis étranger"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.legifrance.gouv.fr/affichTexte.do?cidTexte=JORFTEXT000033418805&dateTexte=&categorieLien=id"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "La loi n°2016-1547 du 18 novembre 2016 de modernisation de la justice du XXIe siècle"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n prévoit d'affecter des points au permis de conduire des titulaires d'un permis étranger circulant en France (\"permis à points virtuel\")."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Ce nombre de points sera réduit si le conducteur commet en France une infraction pour laquelle le retrait de points est prévu."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Un décret doit préciser la date d'entrée en vigueur de ces dispositions."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les informations contenues sur cette page restent d'actualité."
                    }
                  ]
                }
              ]
