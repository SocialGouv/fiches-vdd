        {
          "type": "element",
          "name": "Introduction",
          "children": [
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le Pass Éducation permet à "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "tous les personnels de l’Éducation nationale (enseignants ou non)"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " en école, collège ou lycée d'accéder "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "gratuitement"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " à certains musées et monuments nationaux. Ce Pass est délivré par les directeurs d'école et les chefs d'établissement. Il est valable 3 ans."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "De quoi s'agit-il ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le Pass Éducation concerne les personnels de l’Éducation nationale."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il vous permet de voir gratuitement les collections permanentes des "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "http://www.education.gouv.fr/cid24202/pass-education-gratuite-des-musees-et-monuments-nationaux-pour-les-enseignants.html#Liste_des musées et monuments nationaux en académie"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "musées nationaux"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n\n. Les expositions temporaires restent donc payantes."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ANoter",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Exemple"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le Grand Palais à Paris ne dispose pas de collections permanentes et ne présente que des expositions temporaires, celles-ci sont donc payantes."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il permet également de visiter gratuitement les "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.monuments-nationaux.fr/Trouver-un-monument"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "monuments nationaux"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n\n."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le Pass Éducation se présente sous la forme d'une carte."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Qui peut en bénéficier ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le Pass s'adresse à tous les personnels travaillant en école, collège ou lycée dans un établissement public ou privé sous contrat (par exemple, enseignants, surveillants, personnels administratifs)."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous pouvez en bénéficier si vous êtes fonctionnaire ou contractuel ou "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F18933",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "stagiaire dans le cadre de votre titularisation"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Comment l'obtenir ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le Pass Éducation est distribué par les directeurs d'école et les chefs d'établissement."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous devez vous renseigner auprès du secrétariat de votre établissement. Vous devrez fournir une photo d'identité."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si vous êtes stagiaire, vous devez vous adresser à l'établissement dans lequel vous faites votre stage."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "OuSAdresser",
                  "attributes": {
                    "ID": "R12125",
                    "sve": "non",
                    "type": "Local personnalisable"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Établissement scolaire"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "PivotLocal",
                      "children": [
                        {
                          "type": "text",
                          "text": "etablissement_scolaire"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "RessourceWeb",
                      "attributes": {
                        "URL": "https://www.education.gouv.fr/annuaire"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quel est son coût ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le Pass Éducation est gratuit."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quelle est sa durée de validité ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La Pass Éducation est valable pendant une durée de 3 ans."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Que faire en cas de perte ou vol ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "En cas de perte ou vol de votre Pass Éducation, contactez le directeur d'école ou le chef d'établissement. Ce dernier devra prendre contact avec l'inspection de l’Éducation nationale de sa circonscription."
                    }
                  ]
                }
              ]
            }
