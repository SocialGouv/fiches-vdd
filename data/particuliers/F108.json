                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ANoter",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "À noter"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "la carte est valable jusqu'à la fin de l'année civile en cours, quelle que soit la date de demande."
                        }
                      ]
                    }
                  ]
                }
