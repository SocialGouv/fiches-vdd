        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L'ouverture d'une mesure de protection juridique du majeur (sauvegarde de justice, curatelle, tutelle) peut être demandée au juge des tutelles par les personnes suivantes :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "la personne à protéger elle-même, ou la personne avec qui elle "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R42442",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "vit en couple"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "un parent ou un "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R12901",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "allié"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "une personne entretenant avec le majeur des liens étroits et stables ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "la personne qui exerce (déjà) une mesure de protection juridique (curateur ou tuteur)."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La demande peut être également présentée par le "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R1123",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "procureur de la République"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " qui formule cette demande :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "soit de sa propre initiative ;"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "soit à la demande d'un tiers (par exemple : médecin, directeur d'établissement de santé, travailleur social)."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La mesure de protection juridique est déterminée en fonction du degré d'altération des facultés personnelles de la personne à protéger."
                }
              ]
            }
          ]
        },
