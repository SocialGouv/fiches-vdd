                            },
                            {
                              "type": "element",
                              "name": "RessourceWeb",
                              "attributes": {
                                "URL": "https://lannuaire.service-public.fr/"
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
