        {
          "type": "element",
          "name": "Introduction",
          "children": [
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La grâce présidentielle est un pouvoir dont dispose le Président de la République."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Ce pouvoir lui permet de décider de dispenser une personne condamnée de l'exécution de sa peine."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le Président de la République peut accorder la grâce pour la totalité de la peine ou seulement pour une partie de la peine."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il n'est pas obligé de justifier sa décision d'accorder ou de refuser la grâce à une personne condamnée."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Nous vous présentons les informations à connaître."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Comment distinguer la grâce présidentielle de l'amnistie ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La grâce ne doit pas être confondue avec "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F779",
                        "type": "Fiche Question-réponse conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "l'amnistie"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", car contrairement à l'amnistie :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "La grâce n'efface pas la condamnation du "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R65817",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "casier judiciaire"
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "La grâce concerne une seule personne"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "La grâce n'est pas décidée par une loi votée par le Parlement"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Qui peut bénéficier de la grâce présidentielle ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Pour bénéficier d'une grâce, vous devez être condamné à une peine d'emprisonnement ou à une peine d'amende et la condamnation doit être "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R51889",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "définitive"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La grâce porte uniquement sur les sanctions pénales."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Ainsi, elle ne concerne pas les sanctions suivantes :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Sanctions civiles (par exemple, condamnation à la "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F1422",
                                "type": "Fiche d'information conditionnée",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "réparation du préjudice"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " de la victime ou "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R56424",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "amende civile"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ")"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Sanctions administratives (par exemple, retrait de points du permis de conduire)"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Comment demander la grâce présidentielle ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si vous souhaitez bénéficier de la grâce présidentielle, vous devez en faire la demande par écrit au Président de la République."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "D'autres personnes peuvent aussi introduire la demande pour vous : un membre de votre famille, votre avocat, un ami, un élu ou le Procureur de la République."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "OuSAdresser",
                  "attributes": {
                    "ID": "R44277",
                    "sve": "non",
                    "type": "National"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Présidence de la République"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "RessourceWeb",
                      "attributes": {
                        "URL": "https://lannuaire.service-public.fr/institutions-juridictions/1ad32e5b-c83c-472d-b3cc-6d7453f88b36"
                      }
                    },
                    {
                      "type": "element",
                      "name": "Source",
                      "attributes": {
                        "ID": "R44278"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "Présidence de la République"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le dossier est ensuite étudié par un service spécialisé du ministère de la justice, la Direction des affaires criminelles et des grâces (DACG)."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Ce service recueille l'ensemble des informations et avis de nature à permettre au Président de la République de décider si une grâce peut être envisagée."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si la décision est favorable, un décret de grâce est adopté."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il est signé par le Président de la République et contresigné par le Premier ministre et par le ministre de la justice."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les décrets de grâce ne sont pas publiés au "
                    },
                    {
                      "type": "element",
                      "name": "Expression",
                      "children": [
                        {
                          "type": "text",
                          "text": "Journal officiel"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si un décret de grâce vous concerne, il vous sera directement "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R14732",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "notifié"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", ainsi qu'à la personne qui a sollicité la grâce pour vous."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quels sont les effets de la grâce présidentielle ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La grâce vous dispense d'exécuter la part de la peine pour laquelle elle est accordée, la totalité de la peine, ou une partie de la peine."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La grâce peut aussi remplacer votre peine initiale par une peine plus légère."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La grâce n'a aucun effet sur la décision de condamnation, qui figure toujours à votre "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F14710",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "casier judiciaire"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous pouvez, si vous souhaitez obtenir l'annulation de la condamnation, introduire une demande de "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F1381",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "révision"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " à l'encontre de la décision de condamnation."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La grâce n'empêche pas la victime des "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R10272",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "infractions"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " que vous avez commises d'obtenir "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F1422",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "réparation de son préjudice"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        },
