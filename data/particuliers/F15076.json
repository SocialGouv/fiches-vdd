                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Votre morphologie est manifestement inadaptée au port de celle-ci"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous êtes muni d'un "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "R14006",
                                    "type": "Formulaire",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "certificat médical"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " d'exemption, délivré par un médecin agréé par la préfecture. Le certificat médical doit mentionner sa durée de validité et être présenté au forces de l'ordre en cas de  contrôle."
                                }
