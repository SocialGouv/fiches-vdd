                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "du respect de "
                                            },
                                            {
                                              "type": "element",
                                              "name": "LienExterne",
                                              "attributes": {
                                                "URL": "http://www.driee.ile-de-france.developpement-durable.gouv.fr/agir-ensemble-pour-la-qualite-de-l-air-a3308.html"
                                              },
                                              "children": [
                                                {
                                                  "type": "text",
                                                  "text": "certaines normes d'émissions et de rendement"
                                                }
                                              ]
                                            },
                                            {
                                              "type": "text",
                                              "text": "\n\n si vous utilisez un équipement neuf à  Paris ou  dans certaines "
                                            },
                                            {
                                              "type": "element",
                                              "name": "LienExterne",
                                              "attributes": {
                                                "URL": "http://www.driee.ile-de-france.developpement-durable.gouv.fr/IMG/pdf/2018-_arrete_ppa3_vsigne.pdf",
                                                "format": "application/pdf",
                                                "poids": "420.1 KB"
                                              },
                                              "children": [
                                                {
                                                  "type": "text",
                                                  "text": "communes situées en zone sensible"
                                                }
                                              ]
                                            },
                                            {
                                              "type": "text",
                                              "text": "\n\n,"
                                            }
                                          ]
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "du respect de "
                                            },
                                            {
                                              "type": "element",
                                              "name": "LienExterne",
                                              "attributes": {
                                                "URL": "http://www.driee.ile-de-france.developpement-durable.gouv.fr/agir-ensemble-pour-la-qualite-de-l-air-a3308.html"
                                              },
                                              "children": [
                                                {
                                                  "type": "text",
                                                  "text": "certaines normes d'émissions et de rendement"
                                                }
                                              ]
                                            },
                                            {
                                              "type": "text",
                                              "text": "\n\n si vous utilisez un équipement existant à  Paris,"
                                            }
                                          ]
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": " que cela ne soit pas interdit par le règlement de copropriété si vous vivez en appartement,"
                                            }
                                          ]
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "et que le ramonage ait été effectué conformément aux règles locales (arrêté municipal ou préfectoral consultable en mairie)."
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  ]
