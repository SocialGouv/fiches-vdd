        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Oui, les "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R12469",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "héritiers"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " peuvent désigner un "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R12420",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "mandataire"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " pour gérer la succession. Le mandataire peut être :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "soit désigné par les héritiers ("
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "mandat conventionnel"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "),"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "soit désigné par le juge ("
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "mandat successoral judiciaire"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "), lorsqu'il y un blocage ou lorsqu'un des héritiers a  accepté la succession "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F1199",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "à concurrence de l'actif net"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "BlocCas",
              "attributes": {
                "affichage": "radio"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Mandat conventionnel"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les héritiers peuvent choisir comme mandataire l'un d'entre eux ou toute autre personne."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le mandat peut être fait  par acte notarié ou par acte "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R17852",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "sous seing privé"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le mandataire a pour mission d'administrer la succession du défunt. Il doit rendre compte de sa gestion. "
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le mandataire n'est pas rémunéré sauf indication contraire mentionnée dans le mandat."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Mandataire successoral judiciaire"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Lorsqu'un héritier au moins a accepté la succession à concurrence de l'actif net, seul le juge peut désigner le mandataire."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Un mandataire successoral judiciaire peut également être nommé en cas de blocage de la succession (mésentente des héritiers, succession complexe, etc.)."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Toute personne qualifiée peut être nommée en qualité de mandataire successoral judiciaire. Il peut aussi s'agir d'une personne morale (association, fondation, etc.)."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La désignation d'un mandataire successoral judiciaire peut être demandée par toute personne intéressée  (héritier, créancier, "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R1127",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "ministère public)"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ")."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La demande doit être faite au  par voie "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F20851",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "d’assignation en référé au tribunal"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " du domicile du défunt."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le mandataire successoral judiciaire administre provisoirement la succession. Il réalise les actes conservatoires (règlement des dettes, paiement des impôts, perception des revenus, etc.). Le juge peut l'autoriser à effectuer d'autres types d'actes (par exemple la vente d'un bien immobilier)."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le juge fixe la durée de la mission et la rémunération du mandataire successoral."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le mandat prend fin si les héritiers signent une "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F1296",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "convention d'indivision"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ou désignent un notaire en vue du "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F16194",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "partage de la succession"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
