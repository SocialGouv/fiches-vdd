        {
          "type": "element",
          "name": "ListeSituations",
          "attributes": {
            "affichage": "onglet"
          },
          "children": [
            {
              "type": "element",
              "name": "Situation",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Demande d'une administration française"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Il n'est plus obligatoire de fournir une copie certifiée conforme d'un document venant d'une administration française pour remplir une démarche auprès d'une autre administration française."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les services de l'État (préfecture, université), locaux (mairie...) ou tout organisme public (comme Pôle emploi) sont concernés."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Par exemple, vous n'avez pas besoin de faire certifier la copie de votre bac pour vous inscrire à l'université."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "Une simple photocopie lisible du document original doit être acceptée"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "En cas de doute sur la validité de la copie, l'administration concernée peut vous demander la production de l'original."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'administration doit justifier cette demande par lettre recommandée avec demande d'avis de réception."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Attention",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Attention"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "une autre procédure s'applique en cas de "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F1402",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "légalisation d'un document étranger destiné à une administration française"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " ou de "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F1400",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "légalisation d'un document français destiné à une autorité étrangère"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
