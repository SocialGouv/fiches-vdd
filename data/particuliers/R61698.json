              ]
            },
            {
              "type": "element",
              "name": "Rappel",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Rappel"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "le nouveau système d'information sur les armes (SIA) est accessible aux "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "chasseurs"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " depuis le "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "8 février 2022"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". Il sera progressivement ouvert aux autres détenteurs d'armes particuliers."
                    }
                  ]
                }
              ]
            }
