                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'ancien syndic doit "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F31672",
                            "type": "Fiche Question-réponse conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "transmettre des documents au nouveau syndic"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " sous certains délais."
                        }
                      ]
                    }
                  ]
                }
