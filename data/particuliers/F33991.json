          "children": [
            {
              "type": "text",
              "text": "Peut-on résilier un contrat de prestations de services à tacite reconduction ?"
            }
          ]
        },
        {
          "type": "element",
          "name": "dc:creator",
          "children": [
            {
              "type": "text",
              "text": "Direction de l'information légale et administrative"
            }
          ]
        },
        {
          "type": "element",
