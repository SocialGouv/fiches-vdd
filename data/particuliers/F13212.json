                {
                  "type": "text",
                  "text": " :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "soit les familles n'y ont plus droit (par exemple : "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2550",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "prime à la naissance de la prestation d'accueil du jeune enfant (Paje)"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2552",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "allocation de base de la Paje"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ou "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F13220",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "prime à l'adoption de la Paje"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "),"
                        }
                      ]
                    }
