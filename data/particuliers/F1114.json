                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Solliciter l'avis d'un hydrogéologue (vous devez vous adresser à l'Agence régionale de santé - ARS)"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Obtenir l'accord du préfet en déposant une demande d'inhumation dans une propriété privée (avec l’ensemble de vos justificatifs) à la préfecture du département où est située la propriété."
                                }
                              ]
                            }
                          ]
                        }
                      ]
