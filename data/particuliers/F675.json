                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Inscription d'un étudiant étranger à la Sécurité Sociale"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Source",
                          "attributes": {
                            "ID": "R30675"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Caisse nationale d'assurance maladie (Cnam)"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "ASavoir",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "À savoir"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "si vous êtes étudiant andorran ou québécois vous êtes couvert par la convention bilatérale de sécurité sociale signée par votre pays et la France."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
