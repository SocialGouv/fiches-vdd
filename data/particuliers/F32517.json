                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Fonctionnaire"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Contractuel en "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "R24389",
                                "type": "Sigle"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "CDI"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " dont la rémunération est calculée à partir d'un indice"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Contractuel en "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "R2454",
                                "type": "Sigle"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "CDD"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " employé de manière continue par le même employeur public sur la période de référence et dont la rémunération est calculée à partir d'un indice"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La garantie individuelle du pouvoir d'achat (Gipa) concerne toutes les catégories d'agents (A, B et C)."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Calcul"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "SousChapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Formule"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'indemnité de garantie du pouvoir d’achat (G) est calculée sur la base des "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F461",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "traitements bruts"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " annuels (TBA), selon la formule suivante :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "G = TBA de l'année de début de la période de référence x (1 + inflation sur la période de référence) - TBA de l'année de fin de la période de référence."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les traitements bruts annuels (TBA) pris en compte sont calculés de la manière suivante :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "TBA = IM détenu au 31 décembre de l'année de début et de l'année de fin de la période de référence x Valeur annuelle du point d'indice pour chacune de ces 2 années."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Un agent à temps partiel ou à temps non complet au cours des 4 ans peut bénéficier de la Gipa au prorata du temps travaillé :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "G = TBA de l'année de début de la période de référence x (1 + inflation sur la période de référence) - TBA de l'année de fin de la période de référence x quotité de temps de travail au 31 décembre de l'année de fin de la période de référence."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "SousChapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Éléments de calcul"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Pour la mise en œuvre de l'indemnité en 2019 :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Liste",
                      "attributes": {
                        "type": "puce"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "la période de référence est fixée du 31 décembre 2014 au 31 décembre 2018,"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "la valeur annuelle du point d'indice pour 2014 était de "
                                },
                                {
                                  "type": "element",
                                  "name": "Valeur",
                                  "children": [
