                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Il peut également être accordé à l'agent public qui "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R42442",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "vit avec la mère"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le congé est accordé sur demande de l'agent et sur présentation de l'acte de naissance ou du livret de famille."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le congé n'a pas à être nécessairement pris le jour de la naissance de l'enfant. Il doit seulement être pris à une date proche de l'événement, fixée en accord avec votre administration."
                            }
                          ]
                        }
                      ]
