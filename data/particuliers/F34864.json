                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Contrat de travail à durée indéterminée"
                    }
                  ]
                }
              ]
            }
          ]
        }
