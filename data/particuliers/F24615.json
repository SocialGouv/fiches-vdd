                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "R24583",
                                "type": "Sigle"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "CMSA"
