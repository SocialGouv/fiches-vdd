                                    {
                                      "type": "text",
                                      "text": "s'il est  titulaire de "
                                    },
                                    {
                                      "type": "element",
                                      "name": "LienInterne",
                                      "attributes": {
                                        "LienPublication": "F2208",
                                        "type": "Fiche d'information",
                                        "audience": "Particuliers"
                                      },
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "la carte de résident"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "text",
                                      "text": ","
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "ou s'il a  le statut de "
