              "children": [
                {
                  "type": "text",
                  "text": "Europe Direct"
                }
              ]
            },
            {
              "type": "element",
              "name": "Complement",
              "children": [
                {
                  "type": "text",
                  "text": "Pour toute information"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Informations générales sur l'Union européenne (UE) et sur les organismes à contacter."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Réponses dans toutes les langues officielles de l'UE."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Chapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Par téléphone"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "00 800 6 7 8 9 10 11"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " depuis n'importe quel pays de l'UE."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Ouvert les jours ouvrables de 9h à 18h."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Appel gratuit."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Chapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Par messagerie"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Accès au "
                        },
                        {
                          "type": "element",
                          "name": "LienExterne",
                          "attributes": {
                            "URL": "http://europa.eu/europedirect/write_to_us/index_fr.htm"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "formulaire de contact"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
