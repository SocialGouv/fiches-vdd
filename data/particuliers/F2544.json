                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F10871",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "l'Aspa"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ", les  sommes versées dans le cadre du minimum vieillesse ("
