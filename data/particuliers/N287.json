          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Détenteurs d'armes : accès au système d'information sur les armes (SIA)"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "détenteurs d'armes particuliers majeurs"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " suivants doivent créer un compte dans le SIA :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Personne ayant un "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "permis de chasser"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ", détentrice d'une arme, ou souhaitant acquérir et détenir une arme"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Personne titulaire ou ayant été titulaire d'une "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "licence de la Fédération française de tir pour la pratique du tir, du ball-trap ou du biathlon,"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " détentrice d'une arme, ou souhaitant acquérir et détenir une arme"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Personne "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "souhaitant conserver une arme héritée ou trouvée"
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Personne détentrice ou souhaitant acquérir "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "certaines armes de catégorie C"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " et n'ayant ni permis de chasser, ni licence pour la pratique du tir, du ball-trap ou du biathlon. Il s'agit des armes de catégorie "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "C3"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " (arme à feu fabriquée pour tirer une balle ou plusieurs projectiles non métalliques), de catégorie "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "C9"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " (arme à feu de catégorie A, B ou C "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R66729",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "neutralisée"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ") et "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "C12"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " ("
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R69325",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "arme d'alarme et de signalisation"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ")."
                            }
                          ]
                        }
                      ]
                    }
