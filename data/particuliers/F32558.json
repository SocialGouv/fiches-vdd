                  "name": "Fiche",
                  "attributes": {
                    "ID": "F1480"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "Hériter d'un véhicule"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Fiche",
                  "attributes": {
                    "ID": "F1726"
                  },
                  "children": [
                    {
                      "type": "text",
