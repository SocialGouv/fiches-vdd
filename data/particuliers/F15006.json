          },
          "children": [
            {
              "type": "text",
              "text": "Comment régler un litige avec un avocat ?"
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F15018",
            "audience": "Particuliers"
          },
          "children": [
            {
              "type": "text",
              "text": "Comment sont fixés les honoraires d'un avocat ?"
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F932",
