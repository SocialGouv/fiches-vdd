                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "En ligne"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Adressez votre réclamation sur votre espace Particulier, à la rubrique \"Nous contacter\" de votre messagerie."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "ServiceEnLigne",
                      "attributes": {
