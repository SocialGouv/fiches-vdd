              "children": [
                {
                  "type": "text",
                  "text": "Code civil : article 2412"
                }
              ]
            },
            {
              "type": "element",
              "name": "Complement",
              "children": [
                {
                  "type": "text",
                  "text": "Hypothèques judiciaires"
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Reference",
          "attributes": {
            "type": "Texte de référence",
            "URL": "http://www.legifrance.gouv.fr/affichCode.do?idArticle=LEGIARTI000025026104&idSectionTA=LEGISCTA000025026414&cidTexte=LEGITEXT000025024948",
            "ID": "R33175"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
