              "children": [
                {
                  "type": "text",
                  "text": "Il existe 3 types d'aides personnelles au logement :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
