                  "attributes": {
                    "ID": "N19809"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Loisirs"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Fiche",
              "attributes": {
                "ID": "F1839",
                "audience": "Particuliers"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
