            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "ne pouvez pas"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " vous pacser avec une personne à laquelle vous êtes lié par un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "lien familial direct"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L'interdiction s'applique qu'il s'agisse d'un lien de parenté ou d'un "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R64427",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "lien d'alliance"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous ne pouvez donc pas vous pacser avec les personnes suivantes :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Votre père, votre mère, votre grand-père ou votre grand-mère"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Votre fils, votre fille, votre petit-fils ou votre petite-fille"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Votre frère ou votre sœur"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Votre demi-frère ou votre demi-sœur"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Votre oncle ou votre tante"
