                  "children": [
                    {
                      "type": "text",
                      "text": "Vente d'un logement en copropriété"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Theme",
                  "attributes": {
                    "ID": "N19808"
                  },
                  "children": [
                    {
                      "type": "element",
