                    },
                    {
                      "type": "text",
                      "text": " pour conserver le droit de conduire "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "à l'étranger"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " un camping-car de plus de 3,5 tonnes."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Extension du permis B (code 96)"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La mention additionnelle "
