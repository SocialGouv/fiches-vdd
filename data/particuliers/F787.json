              "children": [
                {
                  "type": "text",
                  "text": "Oui, la "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R2980",
                    "type": "Définition de glossaire"
                  },
