                    },
                    {
                      "type": "element",
                      "name": "Liste",
                      "attributes": {
                        "type": "puce"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "il peut comporter des scènes pouvant choquer les plus jeunes (nudité, violence),"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "il peut aussi comporter un langage légèrement grossier (mais pas d'insultes à caractère sexuel)."
                                }
                              ]
                            }
                          ]
