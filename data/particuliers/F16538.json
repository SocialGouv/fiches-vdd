                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "BlocCas",
                          "attributes": {
                            "affichage": "onglet"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Cas",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Titre",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "Mineur voyageant avec ses parents"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Votre enfant mineur doit être muni d'une pièce d'identité individuelle (passeport valide ou de sa carte d'identité  valide)."
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "ANoter",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Titre",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "À noter"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "le livret de famille n'est pas un titre de voyage."
                                        }
                                      ]
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Cas",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Titre",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "Mineur voyageant seul ou accompagné d'une tierce personne"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Si votre enfant mineur voyage seul ou est accompagné d'une tierce personne, il doit être muni :"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Liste",
                                  "attributes": {
                                    "type": "puce"
                                  },
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "de son passeport valide ou de sa carte d'identité  valide,"
                                            }
                                          ]
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "d'une déclaration de consentement de votre part  dans laquelle figure son  identité (la déclaration de consentement prend la forme d'une lettre,  est revêtue d'une "
                                            },
                                            {
                                              "type": "element",
                                              "name": "LienInterne",
                                              "attributes": {
                                                "LienPublication": "F1411",
                                                "type": "Fiche d'information",
                                                "audience": "Particuliers"
                                              },
                                              "children": [
                                                {
                                                  "type": "text",
                                                  "text": "signature légalisée"
                                                }
                                              ]
                                            },
                                            {
                                              "type": "text",
                                              "text": " et est présentée aux autorités par l'enfant ou son accompagnateur lors de son arrivée en Suisse)"
                                            }
                                          ]
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Item",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "Paragraphe",
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "et d'une "
                                            },
                                            {
                                              "type": "element",
                                              "name": "LienInterne",
                                              "attributes": {
                                                "LienPublication": "F1359",
                                                "type": "Fiche d'information",
                                                "audience": "Particuliers"
                                              },
                                              "children": [
                                                {
                                                  "type": "text",
                                                  "text": "autorisation de sortie du territoire"
                                                }
                                              ]
                                            },
                                            {
                                              "type": "text",
                                              "text": " pour quitter la France."
                                            }
                                          ]
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "ANoter",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Titre",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "À noter"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "le livret de famille n'est pas un titre de voyage."
                                        }
                                      ]
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
