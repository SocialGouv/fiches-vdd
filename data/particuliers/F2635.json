          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Non"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", votre assurance "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2628",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "auto"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2628",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "moto"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2697",
                    "type": "Fiche Question-réponse conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "vélo"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou trottinette électrique ne vous couvre "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "pas automatiquement"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " en cas d'usage professionnel de votre moyen de locomotion."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous devez souscrire une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "garantie spécifique"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " pour être couvert en cas d'utilisation de votre véhicule personnel pour effectuer des déplacements professionnels."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il faut faire la "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "différence"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " entre les "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "trajets domicile-travail"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " et les "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "déplacements professionnels"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "BlocCas",
              "attributes": {
                "affichage": "radio"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Trajets domicile-travail"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le fait de vous rendre à votre travail avec votre voiture, moto, vélo ou trottinette électrique n'est pas un usage professionnel du véhicule."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Mais vous devez vérifier si votre contrat d'assurance autorise sans condition l'usage de votre véhicule pour effectuer ces trajets."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Parfois, les assureurs exigent un supplément de cotisation si vous devez utiliser votre véhicule pour les trajets quotidiens domicile-travail."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si votre contrat n'autorise pas l'usage de votre véhicule pour effectuer les trajets domicile-travail, vous devez demander à votre assureur un avenant qui précise les conditions auxquelles vous pouvez être couvert pour ces trajets."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "ASavoir",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "À savoir"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "l'assurance responsabilité civile vélo "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F2697",
                                "type": "Fiche Question-réponse conditionnée",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "n'est pas toujours obligatoire"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ", contrairement à l'assurance auto, moto et trottinette électrique."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Déplacements professionnels"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Introduction",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le fait d'utiliser votre voiture, moto ou vélo pendant votre temps de travail pour effectuer des trajets pour le compte de votre employeur est un usage professionnel du véhicule."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous devez informer votre assureur et obtenir son accord avant d'effectuer ce genre de déplacement."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Sinon, en cas d'accident, votre assurance peut refuser de vous indemniser, en totalité ou en partie."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "La couverture du risque lié à l'usage de votre véhicule personnel pour des déplacements professionnels peut se faire à votre initiative ou à celle de l'employeur."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "BlocCas",
                      "attributes": {
                        "affichage": "radio"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous souscrivez l'assurance déplacements professionnels"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous pouvez demander à votre assureur de souscrire une extension de garantie "
                                },
                                {
                                  "type": "element",
                                  "name": "Expression",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "utilisation du véhicule à usage professionnel"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si l'assureur accepte, vous serez couvert par votre "
                                },
                                {
                                  "type": "element",
                                  "name": "LienIntra",
                                  "attributes": {
                                    "LienID": "R47105",
                                    "type": "Définition de glossaire"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "responsabilité civile"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " en cas de dommages matériels et/ou corporels causés à un tiers lors des déplacements professionnels."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Toutes les garanties de votre contrat seront alors applicables lors de l'usage professionnel de votre véhicule."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
