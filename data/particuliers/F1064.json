        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le délai de réclamation varie selon le type d'impôt."
                }
              ]
            },
            {
              "type": "element",
              "name": "BlocCas",
              "attributes": {
                "affichage": "onglet"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Impôt sur le revenu"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La réclamation doit intervenir avant le 31 décembre de la 2"
                        },
                        {
                          "type": "element",
                          "name": "Exposant",
                          "children": [
                            {
                              "type": "text",
                              "text": "e"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " année qui suit celle de la mise en recouvrement de l'impôt (indiquée sur l'avis d'imposition)."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Exemple : le délai s'achève le 31 décembre 2022 pour l'impôt sur le revenu mis en recouvrement en 2020."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Impôts locaux"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La réclamation doit intervenir avant le 31 décembre de l'année qui suit celle de la mise en recouvrement de l'impôt indiquée sur l'avis d'imposition."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Exemple : le délai s'achève le 31 décembre 2021 pour la taxe d'habitation mise en recouvrement en 2020."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Impôt sur la fortune immobilière"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La réclamation doit intervenir avant le 31 décembre de la 2"
                        },
                        {
                          "type": "element",
                          "name": "Exposant",
                          "children": [
                            {
                              "type": "text",
                              "text": "e"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " année qui suit celle de la mise en recouvrement de l'impôt, indiquée sur l'avis d'imposition."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Exemple : le délai s'achève le 31 décembre 2022 pour l'impôt sur le revenu mis en recouvrement en 2020."
                        }
                      ]
                    }
                  ]
                }
              ]
