                  "children": [
                    {
                      "type": "text",
                      "text": "Convention collective"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Theme",
                  "attributes": {
                    "ID": "N19806"
                  },
                  "children": [
                    {
                      "type": "element",
