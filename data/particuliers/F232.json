                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "À la fin de la classe de 3"
                    },
                    {
                      "type": "element",
                      "name": "Exposant",
                      "children": [
                        {
                          "type": "text",
                          "text": "e"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " « prépa-métiers », votre enfant participe à la même "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F112",
