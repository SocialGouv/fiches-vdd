            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si vous n'avez pas d'identifiants ANTS, il est proposé de créer un compte pour avoir un espace personnel sur le site de l'ANTS."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "pré-demande en ligne"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " d'un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "permis international "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "est "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "obligatoire"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " si vous avez votre "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R2308",
                    "type": "Définition de glossaire"
                  },
                  "children": [
