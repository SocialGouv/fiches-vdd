                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Durée du préavis en cas de démission du salarié"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Colonne",
                      "attributes": {
                        "largeur": "23",
                        "type": "normal"
