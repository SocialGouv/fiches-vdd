                {
                  "type": "text",
                  "text": "Si vous avez "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "perdu"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " une arme, un élément d'arme ou des munitions des catégories A, B ou C, vous devez immédiatement "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "faire une déclaration"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " au commissariat de police ou à la brigade de gendarmerie."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous devez faire la même démarche en cas de "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "vol"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Ne pas respecter cette règle est sanctionné par une amende de "
                },
                {
                  "type": "element",
                  "name": "Valeur",
                  "children": [
                    {
                      "type": "text",
                      "text": "750 €"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Dans la déclaration, vous devez "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "détailler précisément les circonstances de la perte ou du vol"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " et "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "donner toutes les indications suivantes sur l'arme"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
