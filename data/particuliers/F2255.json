        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "En cas de "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "perte"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou de "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "vol"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " d'une arme, d'un élément d'arme ou de munitions des catégories "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2242",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "A"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2250",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "B"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2246",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "C"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "vous devez le déclarer immédiatement "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "auprès d'un commissariat de police ou d'une brigade de gendarmerie."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Votre déclaration doit détailler précisément les "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "circonstances de la perte ou du vol"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Vous devez aussi préciser la "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "marque"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", le "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "modèle"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", le "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "calibre"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", le "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "numéro de série"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " et la "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "catégorie"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " de l'arme, de l'élément d'arme ou des munitions concernés."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Ne pas faire la déclaration de perte ou de vol est sanctionné par une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "amende"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " de "
                },
                {
                  "type": "element",
                  "name": "Valeur",
                  "children": [
                    {
                      "type": "text",
                      "text": "750 €"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " maximum."
                }
              ]
            },
            {
              "type": "element",
              "name": "OuSAdresser",
              "attributes": {
                "ID": "R13",
                "sve": "non",
                "type": "Local personnalisé sur SP"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Commissariat"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "PivotLocal",
                  "children": [
                    {
                      "type": "text",
                      "text": "commissariat_police"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "RessourceWeb",
                  "attributes": {
                    "URL": "https://www.masecurite.interieur.gouv.fr/fr/trouver-un-commissariat-une-gendarmerie"
                  }
                }
              ]
            },
            {
              "type": "element",
              "name": "OuSAdresser",
              "attributes": {
                "ID": "R59967",
                "sve": "non",
                "type": "Local personnalisé sur SP"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Brigade de gendarmerie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "PivotLocal",
                  "children": [
                    {
                      "type": "text",
                      "text": "gendarmerie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "RessourceWeb",
                  "attributes": {
                    "URL": "http://www.interieur.gouv.fr/Contact/Contacter-une-brigade-de-gendarmerie-ou-un-commissariat-de-police"
                  }
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le commissaire de police ou le commandant de brigade de gendarmerie vous remet un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "récépissé de déclaration de perte ou de vol"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si vous êtes "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "R61698",
                    "type": "Téléservice",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "détenteur d'un compte dans le SIA"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", vous devez renseigner le vol ou la perte de l'arme dans le "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "R61712",
                    "type": "Sigle"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "SIA"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " en produisant la copie du récépissé."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le commissariat ou la gendarmerie transmet ensuite votre déclaration à la préfecture qui vous a accordé l'autorisation ou délivré le récépissé de déclaration."
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À noter"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si l’arme est inscrite sur votre "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F2273",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "carte européenne d’armes à feu (CEAF"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "), vous devez "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "éditer une nouvelle carte"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " via votre "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "R61698",
                        "type": "Téléservice",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "compte SIA"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". Si vous ne disposez pas de compte SIA, vous devez restituer ou faire mettre à jour votre carte dans un "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "délai d’un mois"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " suivant le vol ou la perte de l’arme."
                    }
                  ]
                }
              ]
            }
          ]
