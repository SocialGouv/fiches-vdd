                  "children": [
                    {
                      "type": "text",
                      "text": "La photo d'identité doit avoir été prise il y a "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "moins de 6 mois"
                        }
