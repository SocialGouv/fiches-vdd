                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Une frange peut être acceptée si elle ne recouvre pas les yeux."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Les oreilles doivent être dégagées."
                            }
                          ]
                        }
                      ]
                    }
