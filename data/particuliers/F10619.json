                    },
                    {
                      "type": "text",
                      "text": " et être "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "ressemblante"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". Pour une demande de carte d'identité ou de passeport, il n'y a qu'"
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "une seule photo"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " à fournir."
                    }
                  ]
                }
              ]
            }
          ]
