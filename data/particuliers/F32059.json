                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Analyse de l'état apparent des "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R44506",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "parties communes"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " et des équipements communs"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "État de la situation du "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F2606",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "syndicat de copropriétaires"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " au regard de ses obligations légales et réglementaires"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Analyse des améliorations possibles concernant la gestion technique et patrimoniale de l'immeuble (état de l'immeuble)"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F1615",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Diagnostic de performance énergétique de l'immeuble ou un audit énergétique"
