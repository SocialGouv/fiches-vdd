                              "attributes": {
                                "LienPublication": "F16158",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "procédure d'insalubrité"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "et"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " pour lequel l'administration demande au "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
