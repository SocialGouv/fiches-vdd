                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Union et fédération de professionnels de santé"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
