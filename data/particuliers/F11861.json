                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F34791",
                        "type": "Fiche Question-réponse",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "relevé d'information intégral (RII)"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " pour connaître les informations liées à votre permis de conduire."
                    }
