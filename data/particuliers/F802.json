                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Entre 2 frères, entre 2 sœurs et entre un frère et une sœur"
                                }
                              ]
                            }
                          ]
                        },
