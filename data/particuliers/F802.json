                              "children": [
                                {
                                  "type": "text",
                                  "text": "Entre "
                                },
                                {
                                  "type": "element",
                                  "name": "LienIntra",
                                  "attributes": {
                                    "LienID": "R12668",
                                    "type": "Définition de glossaire"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "ascendants"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " et "
                                },
                                {
                                  "type": "element",
                                  "name": "LienIntra",
                                  "attributes": {
