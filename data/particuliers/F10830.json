              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous vendez votre logement"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Quelles démarches réaliser ?"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Si votre logement est assuré et que vous le vendez, vous devez prévenir votre assureur de cette vente  (ou de son "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R10832",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "aliénation"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "), par lettre recommandée."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Si vous ne le faites pas, vous devrez continuer à payer les primes pour le logement."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous devez  demander à votre assureur "
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "soit la "
                                    },
                                    {
                                      "type": "element",
                                      "name": "LienInterne",
                                      "attributes": {
                                        "LienPublication": "F19083",
                                        "type": "Fiche d'information",
                                        "audience": "Particuliers"
                                      },
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "résiliation de votre contrat"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "text",
                                      "text": ", "
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "soit son transfert vers le nouvel acquéreur."
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Résiliation par l'assureur"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "L'assureur peut résilier le contrat dans les 3 mois qui suivent le jour où l'acquéreur a demandé  la poursuite de l'exécution du contrat après la vente."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Passé ce délai, il perd le droit de résiliation."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Pour résilier le contrat, l'assureur doit informer l'acquéreur par  lettre recommandée avec avis de réception de sa décision. La résiliation interviendra alors 10 jours après la notification de la lettre recommandée à l'acquéreur."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Situation",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous achetez votre logement"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Chapitre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Quelles démarches réaliser ?"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "En tant qu'acquéreur, vous devez vérifier si le bien est assuré au jour du transfert de propriété."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous pourrez alors :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "soit poursuivre le contrat souscrit par le vendeur,"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "soit résilier immédiatement ce contrat  en adressant une lettre à l'assureur du vendeur,"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "soit "
                                    },
                                    {
                                      "type": "element",
                                      "name": "LienInterne",
                                      "attributes": {
                                        "LienPublication": "F2023",
                                        "type": "Fiche d'information",
                                        "audience": "Particuliers"
                                      },
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "prendre une nouvelle assurance"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "text",
                                      "text": " si le bien n'était pas assuré."
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "En règle générale, l'acte de vente du bien immobilier vous informe  de cette transmission du contrat d'assurance et de la possibilité que vous avez de le résilier."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "ANoter",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "À noter"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "si vous souscrivez un nouveau contrat d'assurance habitation, vous devez cependant  "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F19083",
                                    "type": "Fiche d'information",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "résilier"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " personnellement l'ancien contrat si le vendeur a choisi le transfert du contrat."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
