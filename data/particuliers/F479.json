                                        }
                                      ]
                                    },
                                    {
                                      "type": "text",
                                      "text": ")."
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
