        {
          "type": "element",
          "name": "Introduction",
          "children": [
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Une salariée "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "enceinte"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " ou en "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "congé de maternité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " est protégée contre le licenciement."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Pendant son "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "état de grossesse"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " et "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "après le congé de maternité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", l'employeur "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "peut"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", selon le motif, "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "licencier"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " la salariée. Sa protection contre le licenciement est "
                    },
                    {
                      "type": "element",
                      "name": "Expression",
                      "children": [
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "relative"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Pendant le "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "congé de maternité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", l'employeur "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "ne peut pas licencier"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " la salariée. Sa protection contre le licenciement est "
                    },
                    {
                      "type": "element",
                      "name": "Expression",
                      "children": [
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "absolue"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Nous faisons un point sur la réglementation."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quelles démarches la salariée enceinte doit-elle accomplir pour bénéficier de la protection contre le licenciement ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "L'information de l'employeur quant à l'état de grossesse est indispensable pour bénéficier de la protection contre le licenciement."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "BlocCas",
                  "attributes": {
                    "affichage": "radio"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Cas général"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Pour bénéficier de la protection contre le licenciement, la salariée envoie à son employeur un certificat médical justifiant de son état de grossesse. Le certificat mentionne la date présumée de l'accouchement."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Ce courrier est envoyé par lettre recommandée avec avis de réception."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "L'employeur a notifié le licenciement sans savoir que la salariée est enceinte"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Il est possible qu'un licenciement soit prononcé contre la salarié alors qu'elle n'avait pas encore informé son employeur de son état de grossesse."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Dans ce cas, elle peut lui transmettre un certificat médical justifiant de son état de grossesse. Le certificat mentionne la date présumée de l'accouchement."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Ce courrier peut être envoyé par lettre recommandée dans les 15 jours qui suivent la "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R14732",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "notification"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " du licenciement."
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le licenciement devra alors être annulé."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quelles sont les périodes de protection relative contre le licenciement de la salariée ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il existe "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "2 périodes"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " de protection relative contre le licenciement."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "1re période"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La protection relative débute dès que la salarié a informé son employeur de son état de grossesse. Cette première période se termine lorsque la salariée débute son congé de maternité."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "2ème période"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La protection relative est de 10 semaines. Elle débute :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Soit à la fin du congé maternité"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Soit à la fin des congés payés pris immédiatement après le congé de maternité s'ils existent."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'employeur peut-il licencier une salariée pendant la période de protection relative ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Oui. Pendant cette période, le licenciement est possible uniquement en cas de"
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": " faute grave"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " ou si l'employeur est dans"
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": " l'impossibilité de maintenir le contrat de travail pour un motif étranger à la maternité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " (fermeture définitive de l'entreprise par exemple)."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si l'employeur est dans l'impossibilité de maintenir le contrat de travail, la lettre de licenciement doit "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "obligatoirement préciser les raisons"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " qui rendent impossible ce maintien."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quelles sont les périodes de protection absolue contre le licenciement de la salariée ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Au cours de sa maternité, la salariée bénéficie d'une période de protection absolue contre le licenciement. Cette période couvre l'intégralité du "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F2265",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "congé maternité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", que la salarié "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "use ou non de ce congé"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ANoter",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "À noter"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Elle couvre également un arrêt de travail lié à l'état pathologique de grossesse"
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": " attesté par un certificat médical"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'état pathologique peut se situer avant "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "la date présumée"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " de l'accouchement ou après l'accouchement, ou les 2."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le durée du congé maternité est alors augmenté. Cette augmentation est limitée à 2 semaines avant, et 4 semaines après."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La protection absolue suit cette augmentation."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La protection absolue s'applique également pendant les congés payés"
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": " pris immédiatement"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " après le congé maternité (augmenté éventuellement du congé pathologique)."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'employeur peut-il licencier une salariée pendant son congé maternité ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Non, pendant le congé maternité et l'intégralité de la période de protection absolue, l'employeur ne peut pas licencier la salariée, quel que soit le motif."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ANoter",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "À noter"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Il est toutefois possible, si l'employeur et la salariée sont d'accord, de rompre le contrat par "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F19030",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "rupture conventionnelle"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quelles sont les sanctions pour l'employeur s'il ne respecte pas les périodes de protection contre le licenciement de la salariée ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Lorsque l'employeur licencie la salariée sans respecter les périodes de protection, le licenciement "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F1848",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "peut être annulé"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". La salariée doit saisir le conseil des prud'hommes."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si la salariée en fait la demande, elle peut être réintégrée à son poste de travail."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
