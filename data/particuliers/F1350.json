                    {
                      "type": "element",
                      "name": "Expression",
                      "children": [
                        {
                          "type": "text",
                          "text": "multirisques habitation"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " est un contrat d'assurance habitation modulable."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il contient la garantie obligatoire "
