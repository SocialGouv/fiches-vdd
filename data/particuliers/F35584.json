                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Réalisation des travaux"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vos travaux doivent impérativement être réalisés par un professionnel "
                    },
                    {
                      "type": "element",
                      "name": "Expression",
                      "children": [
                        {
                          "type": "text",
                          "text": "Reconnu garant de l'environnement"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " (RGE)."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ServiceEnLigne",
                  "attributes": {
                    "ID": "R39412",
