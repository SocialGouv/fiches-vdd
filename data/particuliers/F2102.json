                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Personnels actifs de la police nationale"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Personnels de surveillance de l'administration pénitentiaire"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Éducateurs de la protection judiciaire de la jeunesse"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Personnels paramédicaux des hôpitaux militaires"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Contrôleurs aériens"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cellule",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Agents de police municipale"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Sapeurs pompiers professionnels"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "- Agents des réseaux souterrains des égouts"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
