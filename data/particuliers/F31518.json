                {
                  "type": "text",
                  "text": "Parties communes réservées à l'usage exclusif d'un copropriétaire (droit de jouissance exclusif)"
                }
              ]
            },
            {
              "type": "element",
              "name": "Fiche",
              "attributes": {
                "ID": "F31513"
              },
