                {
                  "type": "text",
                  "text": "Toutefois, une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
