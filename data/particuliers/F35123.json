        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R12729"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Usage professionnel ou usage local (droit du travail)"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Pratique d'application générale, constante et fixe au sein d'une profession ou d'une localité et à l'origine d'une règle non écrite s'imposant à l'employeur vis-à-vis du salarié"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R51533"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Dispositions conventionnelles"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Convention collective, accord collectif, accord de branche, d'entreprise ou d'établissement applicables en droit du travail. Elles fixent les obligations et les droits de l'employeur et du salarié."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F1054",
            "audience": "Particuliers"
          },
          "children": [
            {
              "type": "text",
              "text": "Quel droit aux heures de recherche d'emploi pendant un préavis de licenciement ?"
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F36444",
            "audience": "Particuliers"
          },
          "children": [
            {
              "type": "text",
              "text": "Un ressortissant européen salarié en France a-t-il les mêmes droits qu'un salarié français ?"
            }
          ]
        }
      ]
