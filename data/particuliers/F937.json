          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Charte du parrainage d'enfants"
                }
              ]
            },
            {
              "type": "element",
              "name": "Source",
              "attributes": {
                "ID": "R30662"
              },
              "children": [
                {
                  "type": "text",
                  "text": "Ministère des solidarités et de la santé"
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R10398"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Représentant légal"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Personne désignée par la loi pour représenter et défendre les intérêts d'une autre personne, qu'elle soit physique (par exemple, un enfant mineur représenté par son père ou par sa mère) ou morale (par exemple, une société représentée par son dirigeant)"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F1628",
            "audience": "Particuliers"
          },
          "children": [
            {
              "type": "text",
              "text": "Quel est le rôle du Défenseur des droits auprès des enfants ?"
            }
          ]
        }
      ]
    }
  ]
