                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Oui"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", un salarié peut bénéficier de congés parentaux successifs en cas de "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "nouvelle naissance "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "ou "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "d'adoption "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "pendant un congé parental d'éducation."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La demande de nouveau congé parental "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2280",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "à temps plein"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2332",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "à temps partiel"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " est ouvert à tout salarié."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le salarié doit avoir au moins "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "1 an d'ancienneté"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " dans l'entreprise."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La demande motivée est à adresser à l'employeur"
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": " au moins 1 mois"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " avant la date à laquelle le salarié devait reprendre son activité."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le salarié peut "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "mettre fin au congé"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " parental avant la date prévue et reprendre son activité initiale en cas de :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Décès de l'enfant"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Ou diminution importante des ressources du foyer."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "ASavoir",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À savoir"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La salariée enceinte pendant son congé parental peut interrompre son congé en cours au profit d'un "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F1254",
                        "type": "Fiche Question-réponse conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "congé maternité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", sous conditions."
                    }
                  ]
                }
              ]
            }
          ]
