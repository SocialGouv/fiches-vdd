              "children": [
                {
                  "type": "text",
                  "text": "Caisse d'assurance retraite et de la santé au travail (Carsat)"
                }
              ]
            },
            {
              "type": "element",
              "name": "Complement",
              "children": [
                {
                  "type": "text",
                  "text": "Pour tout renseignement complémentaire"
                }
              ]
            },
            {
              "type": "element",
              "name": "PivotLocal",
