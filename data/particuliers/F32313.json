                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "titulaire du baccalauréat"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " ou de son équivalence en niveau"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Être inscrit dans un "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "cursus en formation initiale ou en formation continue"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " préparant un diplôme de l'enseignement supérieur délivré au nom de l'État ou être "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "diplômé de l’enseignement supérieur"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " (personnes détentrices d'un doctorat et inscrites dans un programme post-doctoral)"
