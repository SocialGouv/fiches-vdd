        },
        {
          "type": "element",
          "name": "Introduction",
          "children": [
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "L'identification (anciennement appelée immatriculation) est automatique, dès votre naissance si vous êtes né en France. L'affiliation dépend de votre situation professionnelle et de votre lieu de résidence."
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "L'identification"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " consiste à : "
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous inscrire au "
                        },
                        {
                          "type": "element",
                          "name": "LienExterne",
                          "attributes": {
                            "URL": "https://www.insee.fr/fr/metadonnees/definition/c2074"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "répertoire national inter-régimes de l'Assurance maladie (RNIAM)"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Et vous attribuer un "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "numéro unique et personnel"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", appelé "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F33078",
                                "type": "Fiche Question-réponse conditionnée",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "numéro de Sécurité sociale"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ou "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "NIR"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", qui vous permet de vous identifier auprès de la caisse primaire d'Assurance maladie à laquelle vous êtes affilée."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il faut distinguer selon que vous êtes né ou non en France."
                }
              ]
            },
            {
              "type": "element",
              "name": "BlocCas",
              "attributes": {
                "affichage": "radio"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Naissance en France"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "Si vous êtes né en France"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", l'identification s'effectue dès la naissance."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Toutefois, c'est à partir de ses 16 ans que votre enfant prend connaissance de son numéro complet de Sécurité sociale, lorsqu'il reçoit la "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F265",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "carte Vitale"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Son numéro est en effet indiqué sur la carte."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si votre enfant ne possède pas encore de carte Vitale, son numéro de Sécurité sociale (NIR) est indiqué sur votre "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F10662",
                            "type": "Fiche Question-réponse conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "attestation de droits"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence"
                        },
                        {
                          "type": "text",
                          "text": ", que vous pouvez obtenir dans votre compte Ameli."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Naissance à l'étranger"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "Si vous êtes né à l'étranger"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", il faut "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "demander"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ce numéro à l'organisme qui correspond à "
                        },
                        {
                          "type": "element",
                          "name": "LienExterne",
                          "attributes": {
                            "URL": "https://www.ameli.fr/assure/droits-demarches/principes/numero-securite-sociale"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "votre situation"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " (exemple : vous êtes salarié exploitant agricole, il faut contacter la "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "R24583",
                            "type": "Sigle"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "MSA"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ")."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "L'affiliation"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " consiste à vous rattacher à l'organisme compétent pour gérer vos prestations de base en matière d'Assurance maladie."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Elle dépend :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "De votre situation professionnelle"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "Et"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " de votre lieu de résidence."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "C'est pourquoi :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si vous êtes salarié, travailleur indépendant ou exercez en profession libérale, vous relevez de la Caisse nationale d'Assurance maladie (Cnam). Au niveau local, sauf exceptions, vous êtes rattaché à la "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "R15469",
                            "type": "Sigle"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "CPAM"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " de votre "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R46546",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "résidence habituelle"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si vous êtes salarié ou exploitant agricole, vous relevez de la Mutualité sociale agricole (MSA)."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si vous êtes sans emploi ni activité, vous devez prendre contact avec votre caisse d’Assurance maladie."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L’affiliation à une "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "R15469",
                    "type": "Sigle"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "CPAM"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " a également pour conséquence de vous "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "rattacher à une Carsat"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " (caisse assurance retraite et de santé au travail) notamment, pour la gestion de vos "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "droits à la retraite"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À noter"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Toute personne "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "qui refuse délibérément"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " de s'affilier ou qui "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
