        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "baptême civil "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "(également appelé "
                },
                {
                  "type": "element",
                  "name": "Expression",
                  "children": [
                    {
                      "type": "text",
                      "text": "parrainage civil"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou "
                },
                {
                  "type": "element",
                  "name": "Expression",
                  "children": [
                    {
                      "type": "text",
                      "text": "parrainage républicain"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ") n'est prévu par "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "aucun texte"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Il ne crée "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "aucune obligation"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence"
                },
                {
                  "type": "text",
                  "text": ". Il s'agit d'un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "engagement moral"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " d'ordre "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "purement privé"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le baptême civil se pratique dans "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "certaines mairies"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", mais elles ne sont "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "pas obligées "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "de le célébrer."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le baptême civil n'est "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "pas inscrit sur les registres de l'état civil"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " et les justificatifs éventuellement délivrés n'ont pas de valeur au regard de la loi."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il s'agit d'une cérémonie symbolique et laïque qui vise à célébrer la naissance d'un enfant."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La demande de baptême civil se fait généralement auprès du "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "service État civil ou Vie citoyenne"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " de la mairie."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "délais"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", le "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "déroulement de la cérémonie"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou encore les "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "conditions à remplir"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " peuvent varier d’une commune à l’autre."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il peut être nécessaire de fournir certains documents (par exemple : acte de naissance de l’enfant, pièce d’identité des parents et des parrains/marraines, justificatif de domicile)"
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Certaines mairies proposent un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "formulaire à télécharger en ligne"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "prise de rendez-vous préalable"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "OuSAdresser",
              "attributes": {
                "ID": "R1",
                "sve": "non",
                "type": "Local personnalisé sur SP"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Mairie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "PivotLocal",
                  "children": [
                    {
                      "type": "text",
                      "text": "mairie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "RessourceWeb",
                  "attributes": {
                    "URL": "https://lannuaire.service-public.gouv.fr/navigation/mairie"
                  }
                }
              ]
            },
            {
              "type": "element",
              "name": "ASavoir",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À savoir"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les parents peuvent aussi désigner un parrain et/ou marraine comme "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F130",
                        "type": "Fiche Question-réponse conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "tuteurs par testament ou par déclaration devant notaire"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        },
