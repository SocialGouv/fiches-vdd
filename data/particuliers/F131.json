          },
          "children": [
            {
              "type": "text",
              "text": "Démission, licenciement : peut-on travailler ailleurs avant la fin du préavis ?"
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F2614",
            "audience": "Particuliers"
          },
          "children": [
            {
              "type": "text",
              "text": "Arrêt maladie pendant le préavis : quelles conséquences ?"
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F2930",
            "audience": "Particuliers"
          },
          "children": [
            {
              "type": "text",
              "text": "Le salarié peut-il prendre des congés payés pendant son préavis ?"
            }
          ]
        }
