                    },
                    {
                      "type": "element",
                      "name": "BlocCas",
                      "attributes": {
                        "affichage": "radio"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Cas général"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "En cas de séparation, les prestations sont versées au parent "
                                },
                                {
                                  "type": "element",
                                  "name": "MiseEnEvidence",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "chez lequel l'enfant vit"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": ". Ce parent est appelé "
                                },
                                {
                                  "type": "element",
                                  "name": "Expression",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "l'allocataire"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si vous étiez mariés, cette séparation se traduit par un divorce ou une "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F980",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "séparation de droit ou de fait"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si vous étiez en concubinage, cette séparation se traduit par une "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F18766",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "cessation de la vie commune"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Garde alternée"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Des "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F21248",
                                    "type": "Fiche Question-réponse conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "règles particulières"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " s'appliquent en cas de garde alternée."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Ces règles varient selon que vous êtes "
                                },
                                {
                                  "type": "element",
                                  "name": "MiseEnEvidence",
                                  "children": [
                                    {
