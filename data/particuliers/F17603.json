              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Chien de 1re ou 2e catégorie"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si vous envisagez d'avoir un "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F1839",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "chien de 1re ou 2e catégorie"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", vous devez "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "obligatoirement souscrire une assurance "
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "garantissant votre responsabilité civile pour les éventuels dommages que votre chien pourrait causer à des tiers."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les membres de votre famille sont considérés comme des tiers."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous pouvez souscrire une assurance spécifique, dite "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "responsabilité civile vie privée"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si vous avez souscrit une assurance "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "multirisque habitation"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", la "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2123",
                            "type": "Fiche Question-réponse conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "garantie responsabilité civile"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " est incluse dans votre contrat d'assurance habitation."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "En l'absence d'assurance, vous encourez une amende d'un montant maximal de "
                        },
                        {
                          "type": "element",
                          "name": "Valeur",
                          "children": [
                            {
                              "type": "text",
                              "text": "450 €"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
