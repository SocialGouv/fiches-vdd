                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Depuis la France (métropole)"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'interrogation du FCDDV coûte "
                        },
                        {
                          "type": "element",
                          "name": "Valeur",
                          "children": [
                            {
                              "type": "text",
                              "text": "18 €"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Depuis un département d'outre-mer (Dom)"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'interrogation du FCDDV coûte "
                        },
                        {
                          "type": "element",
                          "name": "Valeur",
                          "children": [
                            {
                              "type": "text",
                              "text": "16,28 €"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
