        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Oui"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", le salarié peut démissionner pour élever son enfant "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "après le congé de maternité ou d'adoption"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Dans ce cas, le salarié est dispensé d'effectuer le préavis."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le salarié informe l'employeur au moins "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "15 jours"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " à l'avance de sa volonté de "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "démissionner pour élever un enfant"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " par lettre "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "R46090",
                    "type": "Acronyme"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "RAR"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou remise en mains propres contre "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R52211",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "décharge"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il peut démissionner :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Soit à la fin du "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2265",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "congé de maternité,"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Soit dans les "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "2 mois"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " suivant la "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "naissance de l'enfant ou l'arrivée de l'enfant au foyer"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le salarié n'a pas à verser une indemnité de rupture du contrat de travail à l'employeur en raison du préavis non effectué."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Après la démission, le salarié peut bénéficier d'une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "priorité de réembauche "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "dans l'entreprise pendant "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "1 an"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " pour des emplois correspondants à ses qualifications."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "demande de priorité de réembauche"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " doit être adressée à l'employeur, "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "dans l'année suivant la rupture du contrat de travail"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", par lettre recommandée avec "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "R39324",
                    "type": "Sigle"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "AR"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou remise en mains propres contre "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R52211",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "décharge"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "En cas de réemploi, le salarié bénéficie de tous les avantages qu'il avait acquis avant son départ."
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À noter"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si le salarié démissionne après la naissance ou l'adoption de l'enfant pour "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "travailler dans une autre entreprise"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", il doit effectuer le préavis dans les conditions habituelles de la "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F2883",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "démission"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        },
