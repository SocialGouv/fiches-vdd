        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "De quoi s'agit-il ?"
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les déchets biodégradables de jardin ou de parc, dits "
                },
                {
                  "type": "element",
                  "name": "Expression",
                  "children": [
                    {
                      "type": "text",
                      "text": "déchets"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Expression",
                  "children": [
                    {
                      "type": "text",
                      "text": "verts"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", sont  :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "l'herbe issue de la tonte de pelouse,"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les feuilles mortes,"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les résidus d'élagage,"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les résidus de taille de haies et arbustes,"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les résidus de "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F33298",
                            "type": "Fiche Question-réponse",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "débroussaillage"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ","
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "les épluchures."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Que faire de ses déchets verts ?"
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il est possible :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "de déposer les déchets verts  en déchetterie, à moins qu'une  collecte sélective organisée par la commune existe,"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "ou de les utiliser en   "
                        },
                        {
                          "type": "element",
                          "name": "LienExterne",
                          "attributes": {
                            "URL": "http://www.ademe.fr/particuliers-eco-citoyens/habitation/jardiner-autrement/utiliser-compost-jardin"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "paillage ou en compost individuel"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "\n\n."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il est interdit de brûler à l'air libre ses  "
                },
                {
                  "type": "element",
                  "name": "Expression",
                  "children": [
                    {
                      "type": "text",
                      "text": "déchets verts"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", comme l'ensemble de ses  "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R35513",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "déchets ménagers"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Toutefois, des dérogations  peuvent exister,  dans les communes :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "dépourvus de déchetterie ou de collecte sélective des déchets verts,"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "où s'applique une obligation de débroussaillement ou un plan de prévention des risques incendie de forêt (PPRif)."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Pour connaître le mode de traitement des"
                },
                {
                  "type": "element",
                  "name": "Expression",
                  "children": [
                    {
                      "type": "text",
                      "text": " déchets verts"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " dans votre commune, ou savoir si une dérogation  s'y applique, contactez votre mairie."
                }
              ]
            },
            {
              "type": "element",
              "name": "OuSAdresser",
              "attributes": {
                "ID": "R1",
                "type": "Local personnalisé sur SP"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Mairie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "PivotLocal",
                  "children": [
                    {
                      "type": "text",
                      "text": "mairie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "RessourceWeb",
                  "attributes": {
                    "URL": "https://lannuaire.service-public.fr/"
                  }
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "En cas de non-respect de l'interdiction"
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il est possible d'alerter les services d'hygiène de la mairie  lorsqu'une personne ne respecte pas l'interdiction de brûler ses "
                },
                {
                  "type": "element",
                  "name": "Expression",
                  "children": [
                    {
                      "type": "text",
                      "text": "déchets verts"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " à l'air libre."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Brûler ses déchets verts à l'air libre  peut être puni d'une amende pouvant aller jusqu'à "
                },
                {
                  "type": "element",
                  "name": "Valeur",
                  "children": [
                    {
                      "type": "text",
                      "text": "450 €"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Les voisins incommodés par les odeurs peuvent par ailleurs engager la responsabilité de l'auteur du brûlage pour "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F19299",
                    "type": "Fiche d'information",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "nuisances olfactives"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "VoirAussi",
          "attributes": {
            "important": "non"
          },
          "children": [
