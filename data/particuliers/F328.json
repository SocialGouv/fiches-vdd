                        },
                        {
                          "type": "text",
                          "text": " mis en circulation "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "à partir de 1960"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " est soumis à l'obligation d'un contrôle technique tous les 5 ans."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Par contre, il est dispensé de contrôle technique si sa mise en circulation est "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "antérieure à 1960"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La vignette  attestant du contrôle technique n'est pas apposée sur le pare-brise d'un véhicule de collection."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Attention",
                      "children": [
                        {
                          "type": "element",
