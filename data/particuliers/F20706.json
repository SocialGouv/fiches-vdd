                    },
                    {
                      "type": "element",
                      "name": "PivotLocal",
                      "children": [
                        {
                          "type": "text",
                          "text": "ordre_avocats"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "RessourceWeb",
                      "attributes": {
                        "URL": "https://www.cnb.avocat.fr/fr/annuaire-barreaux"
                      }
                    },
                    {
                      "type": "element",
