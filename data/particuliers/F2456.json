                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "pour un prêt qui finance l'achat d'un logement "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F2961",
                                "type": "Fiche d'information conditionnée",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "en l'état futur d'achèvement (Véfa)"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ", les "
                            },
                            {
                              "type": "element",
                              "name": "Expression",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "intérêts intercalaires"
                                }
                              ]
