            }
          ]
        },
        {
          "type": "element",
          "name": "DossierPere",
          "attributes": {
            "ID": "N144"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Pacte civil de solidarité (Pacs)"
                }
              ]
            },
            {
              "type": "element",
              "name": "Fiche",
              "attributes": {
                "ID": "F1618"
              },
              "children": [
                {
                  "type": "text",
