              "attributes": {
                "ID": "F1926"
              },
              "children": [
                {
                  "type": "text",
                  "text": "Identification et immatriculation d'une association"
                }
              ]
            },
            {
              "type": "element",
              "name": "Fiche",
              "attributes": {
                "ID": "F11966"
              },
              "children": [
                {
                  "type": "text",
