        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Ce formulaire vous permet de "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "demander votre retraite de base auprès d'un seul organisme"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " pour l'ensemble des activités suivantes que vous avez pu exercer en tant que :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Salarié ou agent contractuel de la fonction publique, chef d’entreprise, conjoint collaborateur, artisan, commerçant, industriel, etc. (Assurance retraite)"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Salarié agricole, chef d'exploitation ou d'entreprise agricole, collaborateur et aide familial ("
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "R24583",
                            "type": "Sigle"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "MSA"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ")"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Professionnel libéral non réglementé relevant de l’Assurance retraite"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Ministre des cultes ou religieux (Caisse d'assurance vieillesse invalidité et maladie des cultes - Cavimac)."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Sauf manifestation contraire de votre part, "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "cette demande vaut également pour votre retraite complémentaire obligatoire"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " en tant que :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Chef d’exploitation ou d’entreprise agricole, collaborateur et aide familial"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Chef d’entreprise, conjoint collaborateur, artisan, commerçant, industriel, etc."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous pouvez déposer une demande unique de retraite personnelle "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "si vous remplissez les conditions suivantes"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous avez atteint l’âge légal de départ à la retraite"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Et vous justifiez d’au moins 1 trimestre d’assurance auprès vous de l’Assurance retraite ou de la MSA"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Ou vous justifiez d’au moins 8 trimestres d’assurance avant le 1er janvier 1998 ou d’au moins 1 trimestre après le 1er janvier 1998 auprès de la Cavimac."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Déposez votre demande unique de retraite personnelle "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "au plus tôt 5 mois avant la date que vous avez choisie pour votre départ."
                    }
                  ]
                }
              ]
            }
