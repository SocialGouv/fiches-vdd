                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R18488",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "réclusion"
