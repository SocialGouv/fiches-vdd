                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Devant la cour d'assises, un jury de citoyens tirés au sort participe à la décision sur la condamnation aux côtés des juges professionnels."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Devant la cour criminelle, l'affaire est jugée par 5 juges professionnels (1 président et 4 "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R54662",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "assesseurs"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ")."
                    }
                  ]
                }
              ]
            },
