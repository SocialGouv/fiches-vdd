                    },
                    {
                      "type": "text",
                      "text": " ou la "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "cour d'assises des mineurs"
