        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Comment a-t-on 2 nationalités ou plus ?"
                }
              ]
            },
            {
              "type": "element",
              "name": "BlocCas",
              "attributes": {
                "affichage": "radio"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "À la naissance"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Un enfant se voit reconnaître la nationalité d'un pays d'une des 2 manières suivantes :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Liste",
                      "attributes": {
                        "type": "puce"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Par application du droit du sol, c'est-à-dire parce qu'il naît dans ce pays."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Par application du droit du sang, c'est-à-dire par "
                                },
                                {
                                  "type": "element",
                                  "name": "LienIntra",
                                  "attributes": {
                                    "LienID": "R38490",
                                    "type": "Définition de glossaire"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "filiation"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": ", parce que ses parents ont la nationalité de ce pays."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si l'un de ses parents devient Français, l'enfant devient aussi "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F3068",
                            "type": "Fiche Question-réponse conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Français"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " si les "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "2 conditions"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " suivantes sont remplies :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Liste",
                      "attributes": {
                        "type": "puce"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "L'enfant réside habituellement avec ce parent (ou alternativement en cas de divorce ou de séparation)."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Le nom de l'enfant figure dans le décret de "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F2213",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "naturalisation"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " ou la déclaration du parent."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'enfant peut ainsi être Français et avoir une autre nationalité, ou 2 autres nationalités si ses parents ont une nationalité différente."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Par naturalisation ou déclaration"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La pluri-nationalité peut s'acquérir après la naissance "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "N111",
                            "type": "Dossier",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "par naturalisation ou par déclaration (mariage, ascendant...)"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "En effet, la loi française n'exige pas qu'un étranger devenu Français renonce à sa nationalité d'origine."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La loi française n'exige pas non plus qu'un Français ayant acquis une autre nationalité renonce à la nationalité française."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Quels sont les effets de la plurinationalité ?"
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Un Français binational ou plurinational a tous les "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "droits et obligations"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " attachés à la nationalité française."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Cette règle s'applique de la même manière à un Français ayant acquis une autre nationalité ou à un étranger devenu français."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Toutefois, un Français binational ou plurinational ne peut pas faire prévaloir sa nationalité française lorsqu'il réside sur le territoire de l’État dont il a aussi la nationalité. Il est alors généralement considéré par cet État comme son ressortissant exclusif. Il ne peut donc pas bénéficier de la protection diplomatique de la France."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Réciproquement, un Français binational ou plurinational ne peut pas faire prévaloir sa ou ses autres nationalités lorsqu'il réside en France. Il ne peut alors pas bénéficier de la protection diplomatique de l’État ou des États dont il a aussi la nationalité."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Un étranger en France peut perdre la plurinationalité si un changement de situation modifie sa nationalité en raison de la législation des États concernés ou d'accords internationaux."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Un Français ayant acquis une autre nationalité peut perdre la nationalité française "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F3073",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "par déclaration"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La nationalité française peut être retirée en cas de "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F32827",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "condamnation pour certains crimes et délits"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "VoirAussi",
          "attributes": {
            "important": "non"
          },
          "children": [
            {
              "type": "element",
              "name": "Fiche",
              "attributes": {
                "ID": "F2718",
                "audience": "Particuliers"
              },
              "children": [
                {
