                  "children": [
                    {
                      "type": "text",
                      "text": "Document de circulation pour étranger mineur (DCEM)"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Theme",
                  "attributes": {
                    "ID": "N19804"
                  },
                  "children": [
                    {
                      "type": "element",
