          "attributes": {
            "ID": "R55826",
            "date": "2020-05-12"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Coronavirus Covid-19 : réglementation des déplacements"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Pour faire face à l'épidémie de covid-19 dans le cadre de l'état d'urgence sanitaire,  un "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.legifrance.gouv.fr/affichTexte.do?cidTexte=JORFTEXT000041865329"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "décret n°2020-548 du 11 mai 2020"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n réglemente certains déplacements."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Une "
                    },
                    {
                      "type": "element",
                      "name": "LienExterne",
                      "attributes": {
                        "URL": "https://www.gouvernement.fr/info-coronavirus/ressources-a-partager#section-b1658"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "déclaration"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "\n est exigée quand le déplacement conduit  à sortir à la fois :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "de votre département,"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "et au-delà de 100 km à vol d'oiseau autour de votre lieu de résidence."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "En cas de contrôle, il faut présenter cette déclaration, un justificatif de domicile et tout document justifiant le motif du déplacement."
                    }
                  ]
                }
