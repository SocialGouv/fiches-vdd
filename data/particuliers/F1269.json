        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Oui"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Le "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F10871",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "prêt à taux zéro (PTZ)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", le "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F10793",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "prêt conventionné (PC)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " et le "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F22158",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "prêt d'accession sociale (PAS)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " sont réservés à l'achat, la construction ou la réalisation de travaux dans votre résidence principale."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Pour que le logement soit considéré comme votre résidence principale, vous devez l'occuper "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "au moins 8 mois par an"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Mais il peut tout de même être considéré comme votre résidence principale lorsque vous l’occupez moins de 8 mois par an pour au moins l’un des motifs suivants :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Cas de "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R38200",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "force majeure"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Raison de santé"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Obligation liée à votre activité professionnelle qui implique des déplacements réguliers"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Nécessité absolue de service liée à votre statut ou votre contrat de travail, ou à celui d’au moins 1 des autres personnes devant habiter le logement"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Eloignement avec votre lieu d’activité ou celui d’au moins 1 des autres personnes devant habiter le logement, durant au maximum 3 ans"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Logement mis en location dans l'attente de votre prochain départ à la retraite."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le logement concerné par le PTZ, le PC ou le PAS doit être votre résidence principale :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Soit au plus tard 1 an après son achat ou la fin des travaux"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Soit à partir de votre départ à la retraite, qui doit intervenir au plus tard 6 ans après son achat ou la fin des travaux. Jusqu'à votre départ à la retraite, vous pouvez mettre le logement en location sous "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F1275",
                            "type": "Fiche Question-réponse conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "certaines conditions"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": "."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
