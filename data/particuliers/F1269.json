        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Oui"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Le"
                },
                {
                  "type": "element",
