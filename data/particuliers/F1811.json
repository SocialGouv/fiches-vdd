                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R1079",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Ampliation"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " du décret autorisant votre changement de "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R10114",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "nom de famille"
                            }
                          ]
                        }
                      ]
