                  "attributes": {
                    "LienPublication": "F15252",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "l'aide à la reprise ou à la création d'entreprise (Arce)"
