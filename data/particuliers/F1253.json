                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F20016",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "nouvel accompagnement pour la création ou la reprise d'entreprise (Nacre)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Elles ne sont pas imposables."
                }
              ]
            },
