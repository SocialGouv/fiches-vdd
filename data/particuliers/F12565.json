                            {
                              "type": "element",
                              "name": "Valeur",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "2,02 €"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " par jour"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
