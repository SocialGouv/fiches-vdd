            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Fumer en voiture en présence d'un mineur"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " est sanctionné par une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "amende"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " pouvant aller jusqu'à "
                },
                {
                  "type": "element",
                  "name": "Valeur",
                  "children": [
                    {
                      "type": "text",
                      "text": "750 €"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". En général, il s'agit d'une amende de "
                },
                {
                  "type": "element",
                  "name": "Valeur",
                  "children": [
                    {
                      "type": "text",
                      "text": "135 €"
                    }
