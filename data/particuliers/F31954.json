              "children": [
                {
                  "type": "text",
                  "text": "mjd"
                }
              ]
            },
            {
              "type": "element",
              "name": "RessourceWeb",
              "attributes": {
                "URL": "http://www.annuaires.justice.gouv.fr/lieux-dacces-aux-droits-10111/"
              }
            },
