                      "attributes": {
                        "LienPublication": "F984",
                        "type": "Fiche d'information",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "bourse de collège"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " ou une "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
