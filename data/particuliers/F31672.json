                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "État des comptes des copropriétaires et du "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2606",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "syndicat de copropriétaires"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " après apurement et clôture"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si le syndic ne remet pas les documents exigés dans les délais"
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le nouveau syndic ou le "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F2610",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "président du conseil syndical"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "doit mettre en demeure l'ancien syndic de transmettre les documents"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ". Cette mise en demeure doit être faite par l'un des moyens suivants :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Lettre recommandée avec accusé de réception ou "
                        },
                        {
                          "type": "element",
                          "name": "LienExterne",
                          "attributes": {
                            "URL": "https://www.inc-conso.fr/content/copropriete-des-precisions-sur-la-lettre-recommandee-electronique"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "lettre recommandée électronique"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R57151",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Acte du commissaire de justice (anciennement huissier de justice et commissaire-priseur judiciaire)"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Remise contre récépissé ou émargement"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si l'ancien syndic ne transmet pas les documents dans un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "délai de 8 jours"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " après la mise en demeure, le nouveau syndic ou le président du conseil syndical peut "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F20851",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "faire un recours devant le tribunal"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le juge peut alors ordonner la remise sous "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R16772",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "astreinte"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " des pièces, informations et documents dématérialisés, et le versement de "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R12532",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "dommages et intérêts"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " dus depuis la mise en demeure."
                }
              ]
            }
          ]
        },
