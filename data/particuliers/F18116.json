        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il existe plusieurs fournisseurs d'électricité et de gaz sur le marché de l'énergie."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous pouvez utiliser un téléservice pour trouver un fournisseur sur votre commune :"
                }
              ]
            },
            {
              "type": "element",
              "name": "ServiceEnLigne",
              "attributes": {
                "ID": "R43793",
                "URL": "http://www.energie-info.fr/pratique/liste-des-fournisseurs",
                "audience": "Particuliers",
                "type": "Outil de recherche"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Rechercher un fournisseur d'électricité et/ou de gaz"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Source",
                  "attributes": {
                    "ID": "R30745"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "Médiateur national de l'énergie"
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le changement de fournisseur est "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "gratuit"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "ASavoir",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À savoir"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "vous pouvez changer de fournisseur d'électricité ou de gaz "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "autant de fois que vous le souhaitez"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". Vous pouvez également "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "revenir chez votre ancien fournisseur"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". Il n'y a pas de période minimum d'engagement."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous pouvez choisir 2 fournisseurs différents, l'un pour l'électricité, l'autre pour le gaz ou pour 1 seul fournisseur pour l'électricité et le gaz."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Pour faire votre choix, vous pouvez vous aider d'un comparateur d'offres :"
                }
              ]
            },
            {
              "type": "element",
              "name": "ServiceEnLigne",
              "attributes": {
                "ID": "R43794",
                "URL": "http://comparateur-offres.energie-info.fr/comparateur-offres-electricite-gaz-naturel/criteria.action?profil=particulier",
                "audience": "Particuliers",
                "type": "Outil de recherche"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Comparateur d'offres d'électricité et de gaz"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Source",
                  "attributes": {
                    "ID": "R30745"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "Médiateur national de l'énergie"
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "En changeant de fournisseur, "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "vous ne changez pas de compteur"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " d'électricité ou de gaz."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Une fois le contrat conclu avec le nouveau fournisseur, vous n'avez pas d'autre démarche à faire. La résiliation de votre ancien contrat est "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "automatique"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Jusqu'à la date de changement, votre fournisseur actuel continue à vous fournir en énergie."
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À noter"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "en cas de litige sur l'exécution de votre contrat, vous devez faire une réclamation avec accusé de réception au service clientèle de votre fournisseur. Si le litige persiste vous pouvez "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F20820",
                        "type": "Fiche Question-réponse conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "faire appel au médiateur national de l'énergie"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ". Ce recours est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "gratuit"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        },
