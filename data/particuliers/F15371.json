            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Un étranger ancien combattant de l'armée française peut :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "obtenir une "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2208",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "carte de résident"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ","
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "et acquérir la nationalité française, sur proposition du ministre de la défense, si en tant qu'engagé dans les armées françaises, il a été blessé en mission au cours d'un engagement opérationnel."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L'enfant étranger d'un ancien combattant de l'armée française n'a pas de droits particuliers, que ce soit :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "pour l'obtention d'un visa d'entrée ou d'un titre de séjour  en France,"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "pour l'acquisition de la nationalité française (sauf exceptions,  sur proposition du ministère de la défense, pour l'orphelin       du militaire décédé  en mission au cours d'un engagement opérationnel)."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
