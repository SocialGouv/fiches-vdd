                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Fiche",
                  "attributes": {
                    "ID": "F491"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "Arrêt maladie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Fiche",
                  "attributes": {
                    "ID": "F547"
                  },
                  "children": [
                    {
                      "type": "text",
