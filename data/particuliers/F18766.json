        },
        {
          "type": "element",
          "name": "ListeSituations",
          "attributes": {
            "affichage": "onglet"
          },
          "children": [
            {
              "type": "element",
              "name": "Situation",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Séparation à l'amiable"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'union libre implique la rupture libre. Chacun des concubins peut, à tout moment, reprendre sa liberté."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Aucune règle n'est prévue en cas de rupture acceptée par les 2 concubins."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Pour tout ce qui est commun, les concubins doivent organiser eux-mêmes leur séparation dans les domaines suivants :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Liste",
                      "attributes": {
                        "type": "puce"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Biens matériels (répartition des meubles, voiture)"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Famille (garde ou résidence des enfants)"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Patrimoine (vente d'un ou de plusieurs biens immobiliers)"
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "La rupture peut se faire sans l'intervention du juge."
                        }
                      ]
                    }
                  ]
                }
