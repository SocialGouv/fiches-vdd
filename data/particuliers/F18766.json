                              "children": [
                                {
                                  "type": "element",
                                  "name": "Titre",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Tribunal judiciaire"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "RessourceWeb",
                                  "attributes": {
