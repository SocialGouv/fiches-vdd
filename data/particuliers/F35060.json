            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "Nom de famille, nom de naissance, nom patronymique"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ont la même signification."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si vous avez un motif légitime, vous pouvez "
