                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F838",
                        "type": "Fiche d'information",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "durée normale d'accueil"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " d'un enfant est fixée à 45 heures par semaine (9 heures par jour)."
                    }
                  ]
                },
                {
                  "type": "element",
