              }
            }
          ]
        },
        {
          "type": "element",
          "name": "Reference",
          "attributes": {
            "type": "Texte de référence",
            "URL": "https://www.legifrance.gouv.fr/codes/article_lc/LEGIARTI000027978346",
            "ID": "R70762"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
