                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Dons familiaux de sommes d'argent"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Le rapport fiscal ne comptabilise pas les "
