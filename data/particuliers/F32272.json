              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Donations de plus de 15 ans"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Elles sont exclues du rapport fiscal à condition d'avoir été "
                        },
                        {
                          "type": "element",
                          "name": "MiseEnEvidence",
                          "children": [
                            {
                              "type": "text",
                              "text": "déclarées aux services fiscaux"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", y compris en cas de "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F1265",
                            "type": "Fiche Question-réponse conditionnée",
