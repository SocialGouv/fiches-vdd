            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Pendant les week-ends ou les vacances,"
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": " vous pouvez décider"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": " de partir à l'étranger"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " (Europe et hors Europe) avec votre enfant."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il est d'usage d'informer l'autre parent."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Ce parent "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F1774",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "peut opposer à ce voyage"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "s'il craint que l'enfant ne revienne pas"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " (parent ou enfant binational ou étranger) ou "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "serait en danger "
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "(pays en guerre, risque climatique important)."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "S'il y a un risque que l'enfant ne revienne pas sur le territoire français, il est possible de saisir le "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "R45626",
                    "type": "Sigle"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "Jaf"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " dont dépend la résidence habituelle de l'enfant. Le juge peut prononcer une "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F1774",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "interdiction de sortie du territoire français (IST)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "En cas d'urgence"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", il est possible d'obtenir une "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F1774",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "opposition à sortie du territoire français (OST)"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            }
          ]
