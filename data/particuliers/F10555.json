        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si les parents ne sont pas d'accord sur le "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R10114",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "nom"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " de leur enfant, l'un des parents doit le signaler par écrit à "
                },
                {
                  "type": "element",
                  "name": "LienIntra",
                  "attributes": {
                    "LienID": "R31350",
                    "type": "Définition de glossaire"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "l'officier de l'état civil"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "La démarche doit être faite au plus tard le jour de la "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F961",
                    "type": "Fiche d'information conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "déclaration de naissance"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "En pratique, le parent présente une "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "déclaration de désaccord"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " à l'officier d'état civil "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "avant la naissance de l'enfant."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L'officier d'état civil contrôle la déclaration, met son visa et la rend au parent."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le parent doit "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "présenter ce document le jour de la déclaration de naissance"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " à l'officier d'état civil du lieu de naissance."
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence"
                }
              ]
            },
            {
              "type": "element",
              "name": "OuSAdresser",
              "attributes": {
                "ID": "R1",
                "sve": "non",
                "type": "Local personnalisé sur SP"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Mairie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "PivotLocal",
                  "children": [
                    {
                      "type": "text",
                      "text": "mairie"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "RessourceWeb",
                  "attributes": {
                    "URL": "https://lannuaire.service-public.fr/navigation/mairie"
                  }
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "L'officier de l'état civil donne à l'enfant un "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "double nom, composé des 2 noms des parents accolés par ordre alphabétique"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": "."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Si un parent a lui même un double nom, l'officier ne retient que le 1"
                },
                {
                  "type": "element",
                  "name": "Exposant",
                  "children": [
                    {
                      "type": "text",
                      "text": "er"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " pour composer le nom de l'enfant."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le nom donné à cet enfant s'impose aux enfants suivants du couple."
                }
              ]
            },
            {
              "type": "element",
              "name": "ANoter",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Exemple"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si le nom du père est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "Susini"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " et le nom de la mère est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "Delmas"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", le nom de l'enfant est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "Delmas Susini"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si le nom du père est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "Susini"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " et le nom de la mère est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "Delmas Moreau"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": ", le nom de l'enfant est "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "Delmas Susini"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Rappel",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Rappel"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "un enfant dont la "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R38490",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "filiation"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " est établie à l'égard de chacun des parents "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F10505",
                        "type": "Fiche d'information conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "peut porter le nom du père, ou le nom de la mère, ou un double nom"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                }
              ]
            }
          ]
        },
