        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R44507"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Parties privatives d'une copropriété"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Parties des bâtiments (porte d'entrée, salon, salle de bains...) et des terrains réservés à l'usage exclusif d'un copropriétaire"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R44506"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Parties communes d'une copropriété"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Parties de bâtiments (couloirs, chaudière, canalisation, garde-corps...) et des terrains (jardins, parcs...) affectés à l'usage ou à l'utilité de tous les copropriétaires"
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Reference",
              "attributes": {
                "type": "Texte de référence",
                "URL": "http://www.legifrance.gouv.fr/affichTexte.do?cidTexte=LEGITEXT000006068256",
                "ID": "R15198"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Loi n°65-557 du 10 juillet 1965 fixant le statut de la copropriété des immeubles bâtis"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "Definition",
          "attributes": {
            "ID": "R55263"
          },
          "children": [
            {
              "type": "element",
              "name": "Titre",
              "children": [
                {
                  "type": "text",
                  "text": "Clause"
                }
              ]
            },
            {
              "type": "element",
              "name": "Texte",
              "children": [
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Phrase ou ensemble de phrases contenues dans le texte d'un acte juridique (par exemple, contrat, testament, loi, règlement) définissant les droits et obligations des personnes concernées par cet acte"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "element",
          "name": "QuestionReponse",
          "attributes": {
            "ID": "F2127",
            "audience": "Particuliers"
          },
          "children": [
            {
              "type": "text",
              "text": "Comment se procurer la copie d'un règlement de copropriété ?"
            }
          ]
        },
        {
          "type": "element",
          "name": "CommentFaireSi",
          "attributes": {
            "ID": "F15913"
          },
          "children": [
            {
              "type": "text",
              "text": "J'achète un logement"
            }
          ]
        }
      ]
