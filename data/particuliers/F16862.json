                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F1824",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "médiation pénale"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " réussie ou à une "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F1461",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "composition pénale"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " exécutée, les poursuites sont arrêtées."
