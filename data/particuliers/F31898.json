                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quel est le point de départ du préavis lors d'un licenciement économique ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le licenciement économique est "
                    },
                    {
                      "type": "element",
                      "name": "LienIntra",
                      "attributes": {
                        "LienID": "R14732",
                        "type": "Définition de glossaire"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "notifié"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " par lettre recommandée avec accusé de réception."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La date de 1"
                    },
                    {
                      "type": "element",
                      "name": "Exposant",
                      "children": [
                        {
                          "type": "text",
                          "text": "re"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " présentation de cette lettre recommandée par les services de la Poste fixe le point de départ du préavis de licenciement."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le salarié touche-t-il des indemnités suite à son licenciement économique ?"
