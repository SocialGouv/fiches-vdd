        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Un mineur peut "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "uniquement"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " participer à "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "certains types de jeux"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Loto traditionnel (mises dont la valeur est inférieure à "
                        },
                        {
                          "type": "element",
                          "name": "Valeur",
                          "children": [
                            {
                              "type": "text",
                              "text": "20 €"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ") appelé "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "poules au gibier"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "rifles"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ou "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "quines"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Tombola"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Loterie dans un "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F21565",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "cadre associatif"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " ou lors d'une fête foraine."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Par contre, un mineur"
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": " ne peut pas participer"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " aux jeux suivants :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Jeux en ligne"
