                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Notaire"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "PivotLocal",
                      "children": [
                        {
                          "type": "text",
                          "text": "notaire"
                        }
                      ]
                    },
