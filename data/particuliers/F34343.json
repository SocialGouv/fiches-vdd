                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Conditions pour en bénéficier"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Pour en bénéficier, vous devez à la rentrée universitaire, remplir l’ensemble des conditions suivantes :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "avoir obtenu une licence l'année précédente,"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "vous inscrire pour la 1"
                            },
                            {
                              "type": "element",
                              "name": "Exposant",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "re"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " fois en 1"
                            },
                            {
                              "type": "element",
                              "name": "Exposant",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "re"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " année de Master,"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "être bénéficiaire de la "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F12214",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "bourse sur critères sociaux"
