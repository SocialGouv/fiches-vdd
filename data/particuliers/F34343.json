                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Avoir obtenu une licence l'année précédente. Attention, l'obtention d'une licence professionnelle ne vous permet pas de bénéficier de l'aide."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous inscrire pour la 1"
                            },
                            {
                              "type": "element",
                              "name": "Exposant",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "re"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " fois en 1"
                            },
                            {
                              "type": "element",
                              "name": "Exposant",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "re"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " année de Master"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Bénéficier de la "
