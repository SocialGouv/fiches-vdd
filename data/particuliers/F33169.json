            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Vous pouvez consulter votre copie sur place (au centre d'examen) ou en ligne."
                }
              ]
            },
            {
              "type": "element",
              "name": "BlocCas",
              "attributes": {
                "affichage": "onglet"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Sur place"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Pour consulter votre copie, vous devez en faire la demande par écrit au centre d'examen. Si vous êtes mineur, la demande doit être faite par votre représentant légal."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les coordonnées du centre d'examen sont généralement indiquées sur votre convocation."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "ASavoir",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "À savoir"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "les centres de formation et d'enseignement supérieur "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "privés"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " ne sont pas obligés d'accepter votre demande de consultation de copie."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous pouvez consulter votre copie sur place, sans pouvoir l'emmener."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous pouvez demander à consulter votre copie pendant un délai d'1 an à partir de la publication des résultats. À la fin de ce délai, les copies sont détruites."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "En ligne"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous pouvez consulter votre copie en ligne en vous connectant sur la plate-forme Cyclades."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "ServiceEnLigne",
                      "attributes": {
                        "ID": "R62913",
                        "URL": "https://exacyc.orion.education.fr/cyccandidat/portal/login#",
                        "type": "Téléservice"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Consulter sa copie d'examen"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Source",
                          "attributes": {
                            "ID": "R30638"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "Ministère chargé de l'éducation"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les dates de mise à disposition des copies peuvent varier selon les académies."
                        }
                      ]
                    }
                  ]
                }
              ]
