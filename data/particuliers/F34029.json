                          "children": [
                            {
                              "type": "text",
                              "text": "le "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F10705",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "compte personnel de formation (CPF)"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ","
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "le "
