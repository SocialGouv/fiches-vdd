                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F2559",
                                "type": "Fiche Question-réponse conditionnée",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "lorsque le locataire ne respecte pas certaines règles ou ne remplit plus certaines conditions"
                                }
                              ]
                            },
