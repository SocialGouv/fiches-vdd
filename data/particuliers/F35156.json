                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Étranger de l'UE, EEE, Confédération Suisse qui réside en France"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Condition",
                      "children": [
                        {
                          "type": "element",
                          "name": "estVrai",
                          "attributes": {
                            "var": "T10010"
                          }
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Un étranger citoyen de "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R46210",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "l'UE, l'EEE, ou Suisse"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " n'est pas obligé d'être titulaire d'un titre de séjour pour être rejoint par sa famille. Il doit cependant justifier des conditions de résidence en France."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Si vous résidez en France "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2651",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "en tant que travailleur, inactif ou étudiant"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": ", vous pouvez être rejoint par les personnes suivantes :"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Liste",
                      "attributes": {
                        "type": "puce"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Votre époux ou épouse"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "vos enfants de moins de 21 ans ou à charge et ceux de votre époux ou épouse"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Item",
                          "children": [
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vos ascendants à charge (père ou mère qui ne peut pas justifier des ressources nécessaires à ses propres besoins) et ceux de votre époux ou épouse"
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Vous devez appartenir à l'une des catégories suivantes et pouvoir en justifier : "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2651",
                            "type": "Fiche d'information conditionnée",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "salarié, inactif, étudiant."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Votre famille doit entrer sur le territoire avec un visa (ou sans visa si elle en est dispensée) et demander un "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "N110",
                            "type": "Dossier",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "titre de séjour"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " auprès de la préfecture de votre département."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Étranger d'un pays tiers qui réside en France avec un titre de séjour"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Condition",
                      "children": [
                        {
                          "type": "element",
                          "name": "estVrai",
                          "attributes": {
                            "var": "T10010"
                          }
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "BlocCas",
                      "attributes": {
                        "affichage": "radio"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous avez un titre de séjour mention \"passeport talent\""
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si vous êtes titulaire d'un titre de séjour mention \""
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F16922",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "passeport talent"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "\", vous pouvez bénéficier du dispositif \"famille accompagnante\" en passant par la procédure de "
                                },
                                {
                                  "type": "element",
                                  "name": "LienExterne",
                                  "attributes": {
                                    "URL": "http://accueil-etrangers.gouv.fr/regroupement-familial/vous-avez-une-carte-de-sejour-pluriannuelle-portant-la-mention-passeport-talent/vous-avez-une-carte-de-sejour-34/"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "famille accompagnante d'un titulaire de passeport talent"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "\n\n."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous avez un titre de séjour de réfugié, protection subsidiaire ou apatride"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si vous êtes titulaire d'un titre de séjour de réfugié, protection subsidiaire, ou apatride, vous pouvez être rejoint par votre famille dans le cadre d'une "
                                },
                                {
                                  "type": "element",
                                  "name": "LienExterne",
                                  "attributes": {
                                    "URL": "https://www.ofpra.gouv.fr/fr/protection-etat-civil/reunification-familiale"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "réunification familiale"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": "\n\n."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous avez un titre de séjour d'une validité d'au moins un an"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous pouvez effectuer une demande de "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "F11166",
                                    "type": "Fiche d'information conditionnée",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "regroupement familial"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": " pour votre époux ou épouse ou vos enfants mineurs."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Cas",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Étranger d'un pays tiers présent en France sans titre de séjour français"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Condition",
                      "children": [
                        {
                          "type": "element",
                          "name": "estVrai",
                          "attributes": {
                            "var": "T12322"
                          }
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Un étranger d'un pays tiers présent en France, mais sans titre de séjour délivré par la France, ne peut pas demander à être rejoint par sa famille."
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
