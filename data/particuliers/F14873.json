              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Voyage d'un majeur"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "BlocCas",
                      "attributes": {
                        "affichage": "radio"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Français"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Pour entrer sur le territoire marocain, vous devez posséder un "
                                },
                                {
                                  "type": "element",
                                  "name": "LienInterne",
                                  "attributes": {
                                    "LienPublication": "N360",
                                    "type": "Dossier",
                                    "audience": "Particuliers"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "passeport en cours de validité"
                                    }
                                  ]
                                },
                                {
                                  "type": "text",
                                  "text": ", valable au minimum jusqu'à la fin de votre séjour."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Aucun visa n'est exigé."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si vous souhaitez rester sur le territoire marocain pour une durée supérieure à 3 mois, mettez-vous directement en relation avec l'ambassade du Maroc en France."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "OuSAdresser",
                              "attributes": {
                                "ID": "R1148",
                                "sve": "non",
                                "type": "Local personnalisable"
                              },
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Titre",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Ambassade ou consulat du Maroc en France"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "PivotLocal",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "ambassade_maroc"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "RessourceWeb",
                                  "attributes": {
                                    "URL": "http://www.diplomatie.gouv.fr/fr/le-ministere-et-son-reseau/annuaires-et-adresses-du-maedi/ambassades-et-consulats-etrangers-en-france/"
                                  }
                                },
                                {
                                  "type": "element",
                                  "name": "Source",
                                  "attributes": {
                                    "ID": "R30604"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Ministère chargé de l'Europe et des affaires étrangères"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Attention",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Titre",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Attention"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "l'entrée sur le territoire marocain n'est plus possible sur la présentation de la seule carte d'identité."
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Franco-marocain"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Vous êtes soumis, lors de votre séjour, aux textes applicables aux ressortissants marocains."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Renseignez-vous sur les conditions d'entrée  et de sortie du Maroc auprès de l'autorité consulaire marocaine compétente."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "OuSAdresser",
                              "attributes": {
                                "ID": "R1148",
                                "sve": "non",
                                "type": "Local personnalisable"
                              },
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Titre",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Ambassade ou consulat du Maroc en France"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "PivotLocal",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "ambassade_maroc"
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "RessourceWeb",
                                  "attributes": {
                                    "URL": "http://www.diplomatie.gouv.fr/fr/le-ministere-et-son-reseau/annuaires-et-adresses-du-maedi/ambassades-et-consulats-etrangers-en-france/"
                                  }
                                },
                                {
                                  "type": "element",
                                  "name": "Source",
                                  "attributes": {
                                    "ID": "R30604"
                                  },
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Ministère chargé de l'Europe et des affaires étrangères"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Même si vous êtes entré au Maroc avec un passeport marocain, pour rentrer en France, il convient de présenter un document d'identité français en cours de validité (passeport ou carte nationale d'identité)."
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Situation",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "Voyage d'un mineur"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Texte",
                  "children": [
                    {
                      "type": "element",
                      "name": "BlocCas",
                      "attributes": {
                        "affichage": "onglet"
                      },
                      "children": [
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Mineur voyageant avec ses 2 parents"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Les parents et le mineur  doivent avoir chacun un passeport en cours de validité. Aucun autre document n'est exigé. La durée de validité du passeport doit couvrir la totalité du séjour."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Toutefois, il est vivement recommandé de prendre  avec vous l'acte de naissance (extrait avec filiation ou copie intégrale) de votre enfant ou votre livret de famille."
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Mineur voyageant avec l'un de ses 2 parents"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "L'enfant doit être muni de son passeport. Son passeport doit être en cours de validité et couvrir toute la durée de son voyage."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si l'enfant voyage avec sa mère seulement, il est recommandé de se munir de l'autorisation du père."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "En cas de divorce des parents, il est recommandé que celui qui accompagne l'enfant soit muni d'une copie du jugement de divorce lui accordant la garde."
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Si l'enfant est orphelin de père et voyage avec sa mère, celle-ci doit être munie :"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Liste",
                              "attributes": {
                                "type": "puce"
                              },
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Item",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "d'un document attestant de son mariage avec le père défunt,"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Item",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "ou d'une copie de l'acte de décès de ce dernier,"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Item",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "ou, à défaut, d'une autorisation du juge chargé de la protection des mineurs."
                                        }
                                      ]
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Cas",
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Mineur voyageant seul ou avec un tiers"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Paragraphe",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "L'enfant doit être muni des 3 documents suivants :"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Liste",
                              "attributes": {
                                "type": "puce"
                              },
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Item",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "Passeport qui doit être en cours de validité et couvrir toute la durée de son voyage"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Item",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "element",
                                          "name": "LienInterne",
                                          "attributes": {
                                            "LienPublication": "F1359",
                                            "type": "Fiche d'information conditionnée",
                                            "audience": "Particuliers"
                                          },
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "Autorisation de sortie de territoire"
                                            }
                                          ]
                                        },
                                        {
                                          "type": "text",
                                          "text": " pour quitter la France"
                                        }
                                      ]
                                    }
                                  ]
                                },
                                {
                                  "type": "element",
                                  "name": "Item",
                                  "children": [
                                    {
                                      "type": "element",
                                      "name": "Paragraphe",
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "Autorisation de ses parents revêtue de leur  "
                                        },
                                        {
                                          "type": "element",
                                          "name": "LienInterne",
                                          "attributes": {
                                            "LienPublication": "F1411",
                                            "type": "Fiche d'information",
                                            "audience": "Particuliers"
                                          },
                                          "children": [
                                            {
                                              "type": "text",
                                              "text": "signature légalisée"
                                            }
                                          ]
                                        },
                                        {
                                          "type": "text",
                                          "text": ". L'autorisation des parents prend la forme d'une lettre. Ce document est à présenter, par l'enfant ou son accompagnateur, lors de son arrivée au Maroc."
                                        }
                                      ]
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
