        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Il est interdit d'attirer ou de nourrir "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "systématiquement ou de façon habituelle"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " des animaux, notamment les pigeons, "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "quand cette pratique est une cause d'insalubrité."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le préfet peut, par arrêté, interdire, "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "sur les territoires et pour la durée qu'il définit"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": ", le nourrissage d'animaux de la faune sauvage, notamment les pigeons."
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Cette interdiction a pour but de prévenir la transmission de maladies entre animaux de la faune sauvage et "
                },
                {
                  "type": "element",
                  "name": "LienInterne",
                  "attributes": {
                    "LienPublication": "F34904",
                    "type": "Fiche Question-réponse conditionnée",
                    "audience": "Particuliers"
                  },
                  "children": [
                    {
                      "type": "text",
                      "text": "animaux domestiques ou de compagnie"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " ou entre animaux de la faune sauvage et humains."
                }
              ]
            }
          ]
