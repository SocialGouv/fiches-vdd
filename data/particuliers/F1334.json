        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "De quoi s'agit-il ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les aides du FSL (sous forme de subvention ou de prêt)  peuvent notamment permettre de financer  :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "les dépenses liées à l'entrée dans le logement : "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F31269",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "dépôt de garantie"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ",\n1"
                            },
                            {
                              "type": "element",
                              "name": "Exposant",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "er"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "  loyer, "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F375",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "frais d'agence"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ", frais de  déménagement, "
                            },
                            {
                              "type": "element",
                              "name": "LienInterne",
                              "attributes": {
                                "LienPublication": "F1349",
                                "type": "Fiche d'information",
                                "audience": "Particuliers"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "assurance"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ", achat du mobilier de 1"
                            },
                            {
                              "type": "element",
                              "name": "Exposant",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "ère"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": " nécessité, ..."
                            }
                          ]
                        }
                      ]
