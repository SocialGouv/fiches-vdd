            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Heures d'absence"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "BlocCas",
                  "attributes": {
                    "affichage": "onglet"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Établissement d'au moins 11 salariés"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le défenseur syndical dispose du temps nécessaire à l'exercice de ses fonctions, dans la limite légale de "
                            },
                            {
                              "type": "element",
                              "name": "MiseEnEvidence",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "10 heures par mois"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Établissement inférieur à 11 salariés"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Aucun crédit d'heures légal n'est imposé à l'employeur."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "En l'absence de "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R51533",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "dispositions conventionnelles"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": ",  le salarié se met d'accord avec l'employeur pour bénéficier d'heures d'absence."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Rémunération"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les absences du défenseur syndical liées à ses activités sont rémunérées par l'employeur, sans diminution de salaire."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Formation"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le défenseur syndical bénéficie d'autorisations d'absence pour les besoins de sa formation."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le défenseur a droit à 2 semaines d'autorisation d'absence par période de 4 ans suivant la publication de la liste des défenseurs syndicaux sur laquelle il salarié est inscrit."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le défenseur syndical informe l'employeur de son absence pour formation au moins 30 jours à l'avance (ou 15 jours si l'absence est inférieure à 3 jours consécutifs). Il précise le nom de l'établissement ou de l'organisme chargé de la formation, la date, la durée et les horaires prévus."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le salarié reçoit une attestation de présence à la formation. Il remet cette attestation à l'employeur à la reprise du travail."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Garanties et obligations"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "SousChapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Congés payés"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les heures d'absence sont prises en compte pour l'acquisition des jours de congés payés."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "SousChapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Ancienneté"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Les heures d'absence sont prises en compte pour déterminer les droits liés à l'ancienneté."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "SousChapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Rupture du contrat"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'exercice de la mission de défenseur syndical ne peut être une cause de sanction disciplinaire ou de rupture du contrat de travail."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le "
                        },
                        {
                          "type": "element",
                          "name": "LienInterne",
                          "attributes": {
                            "LienPublication": "F2406",
                            "type": "Fiche d'information",
                            "audience": "Particuliers"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "licenciement"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " du défenseur syndical doit être autorisé par l'inspecteur du travail."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "SousChapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Obligation de discrétion"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le défenseur syndical est tenu à une obligation de discrétion à l'égard des informations présentant un caractère confidentiel auxquelles il a accès."
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le non respect de ces obligations peut entraîner sa radiation de la liste des défenseurs syndicaux sur laquelle il est inscrit."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "SousChapitre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Obligation d'exercer"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Le défenseur syndical qui n'exerce pas ses missions pendant 1 an est retiré d'office de la liste des défenseurs syndicaux (sauf motif légitime)."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Coût pour le salarié assisté ou représenté"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le défenseur syndical intervient obligatoirement à titre gratuit (sous peine d'être radié de la liste des défenseurs syndicaux sur laquelle il est inscrit)."
                    }
                  ]
                }
              ]
            }
