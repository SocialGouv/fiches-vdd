                      "type": "element",
                      "name": "Variable",
                      "children": [
                        {
                          "type": "text",
                          "text": "indiquez votre revenu mensuel"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " net par mois"
                    }
                  ]
