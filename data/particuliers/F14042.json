        },
        {
          "type": "element",
          "name": "Texte",
          "children": [
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Un test génétique est autorisé "
                },
                {
                  "type": "element",
                  "name": "MiseEnEvidence",
                  "children": [
                    {
                      "type": "text",
                      "text": "uniquement dans le cadre d'une procédure judiciaire"
                    }
                  ]
                },
                {
                  "type": "text",
                  "text": " visant l'un des objectifs suivants :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Établir ou contester un lien de "
                        },
                        {
                          "type": "element",
                          "name": "LienIntra",
                          "attributes": {
                            "LienID": "R38490",
                            "type": "Définition de glossaire"
                          },
                          "children": [
                            {
                              "type": "text",
                              "text": "filiation"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Recevoir ou supprimer une contribution financière (aussi appelée subsides)"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Établir l'identité d'une personne décédée, dans le cadre d'une enquête de police"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le test de paternité peut être accompli selon l'une des méthodes suivantes :"
                }
              ]
            },
            {
              "type": "element",
              "name": "Liste",
              "attributes": {
                "type": "puce"
              },
              "children": [
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Examen comparé des sangs"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Item",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Identification par les empreintes génétiques (test ADN)"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Paragraphe",
              "children": [
                {
                  "type": "text",
                  "text": "Le test de paternité peut être effectué uniquement par des techniciens spécialement agréés à cet effet."
                }
              ]
            },
            {
              "type": "element",
              "name": "ASavoir",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "text",
                      "text": "À savoir"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "aucun test de paternité ne peut être pratiqué avant la naissance."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Peut-on refuser de se soumettre à un test de paternité ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Une personne peut refuser de se soumettre à un test de paternité."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Son consentement est obligatoire pour y procéder."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Toutefois, le juge peut considérer le refus comme une preuve de sa paternité (ou de sa non-paternité, par exemple en cas de reconnaissance frauduleuse)."
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quand peut-on demander au juge un test de paternité ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous pouvez "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "demander au juge un test de paternité"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": " si vous le saisissez pour établir ou contester un lien de filiation."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le test peut être refusé par le juge uniquement pour un motif légitime."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous n'avez pas à réunir des preuves ou indices de la paternité pour obtenir le test."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Vous ne pouvez pas demander un test génétique en urgence, devant "
                    },
                    {
                      "type": "element",
                      "name": "LienInterne",
                      "attributes": {
                        "LienPublication": "F1378",
                        "type": "Fiche Question-réponse conditionnée",
                        "audience": "Particuliers"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "le juge des référés"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "ASavoir",
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "À savoir"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "L'expertise biologique "
                        },
                        {
                          "type": "element",
                          "name": "Expression",
                          "children": [
                            {
                              "type": "text",
                              "text": "post mortem"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " est possible uniquement si la personne avait donné son accord exprès de son vivant."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le test de paternité est ordonné par le juge. L'assistance d'"
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "un avocat est obligatoire"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "OuSAdresser",
                  "attributes": {
                    "ID": "R14218",
                    "sve": "non",
                    "type": "Local"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Avocat"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "RessourceWeb",
                      "attributes": {
                        "URL": "https://www.cnb.avocat.fr/annuaire-des-avocats-de-france"
                      }
                    },
                    {
                      "type": "element",
                      "name": "Source",
                      "attributes": {
                        "ID": "R30762"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "Conseil national des barreaux (CNB)"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "OuSAdresser",
                  "attributes": {
                    "ID": "R30",
                    "sve": "non",
                    "type": "Local personnalisé sur SP"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Titre",
                      "children": [
                        {
                          "type": "text",
                          "text": "Tribunal judiciaire"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "PivotLocal",
                      "children": [
                        {
                          "type": "text",
                          "text": "tgi"
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "RessourceWeb",
                      "attributes": {
                        "URL": "http://www.annuaires.justice.gouv.fr/"
                      }
                    },
                    {
                      "type": "element",
                      "name": "Source",
                      "attributes": {
                        "ID": "R30663"
                      },
                      "children": [
                        {
                          "type": "text",
                          "text": "Ministère chargé de la justice"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Quelles sont les sanctions en cas de test hors du cadre légal ?"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Réaliser un test de paternité "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "en dehors de ce cadre est illégal"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Il est interdit de procéder à un test de paternité dans un cadre privé, par exemple, sur internet ou à l'étranger."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "La réalisation d'un test de paternité hors des règles légales est puni d'une peine "
                    },
                    {
                      "type": "element",
                      "name": "MiseEnEvidence",
                      "children": [
                        {
                          "type": "text",
                          "text": "d'un an d'emprisonnement ou de "
                        },
                        {
                          "type": "element",
                          "name": "Valeur",
                          "children": [
                            {
                              "type": "text",
                              "text": "15 000 €"
                            }
                          ]
                        },
                        {
                          "type": "text",
                          "text": " d'amende"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "text": "."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Les faits suivants sont punis de la même façon :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Diffuser des informations sur l'identification d'une personne par ses empreintes génétiques"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
