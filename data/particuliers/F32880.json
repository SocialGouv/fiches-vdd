            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Démarche"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Le renouvellement peut se faire par internet ou au guichet."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "BlocCas",
                  "attributes": {
                    "affichage": "onglet"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Par internet"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous pouvez utiliser ce  site internet :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "ServiceEnLigne",
                          "attributes": {
                            "ID": "R34754",
                            "URL": "https://www.demande-logement-social.gouv.fr",
                            "type": "Téléservice"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Titre",
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Demande de logement social en ligne"
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Source",
                              "attributes": {
                                "ID": "R30685"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "Ministère chargé du logement"
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous devez cliquer sur \"Je gère ma demande\", puis vous "
                            },
                            {
                              "type": "element",
                              "name": "LienIntra",
                              "attributes": {
                                "LienID": "R48788",
                                "type": "Définition de glossaire"
                              },
                              "children": [
                                {
                                  "type": "text",
                                  "text": "identifier avec France connect"
                                }
                              ]
                            },
                            {
                              "type": "text",
                              "text": "."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous devez fournir :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "le scan de votre pièce d'identité (carte d'identité recto verso ou passeport)"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "si vous êtes étranger, un titre de séjour attestant de la régularité du séjour sur le territoire français"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Si vous avez changé de situation (adresse, revenus, situation familiale, personnes à charge, état de santé...), indiquez-le et fournissez le  justificatif scanné. Par exemple : jugement d'expulsion, lettre de préavis (congé) du propriétaire du logement que vous louez, arrêté préfectoral d'interdiction d'habiter, certificat de grossesse, carte d'invalidité, jugement de divorce ou de non-conciliation..."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Votre renouvellement effectué, une attestation de renouvellement vous est remise dans un délai d'1 mois par messagerie électronique (mail)."
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Au guichet"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le renouvellement peut s'effectuer auprès des  guichets suivants :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Soit auprès d'un bailleur social ou d'un service de l’État (éventuellement  la mairie). Il existe une "
                                    },
                                    {
                                      "type": "element",
                                      "name": "LienInterne",
                                      "attributes": {
                                        "LienPublication": "R54377",
                                        "type": "Outil de recherche",
                                        "audience": "Particuliers"
                                      },
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "liste des  guichets où déposer sa demande de logement social"
                                        }
                                      ]
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Soit auprès d'une   "
                                    },
                                    {
                                      "type": "element",
                                      "name": "LienExterne",
                                      "attributes": {
                                        "URL": "https://www.actionlogement.fr/implantations"
                                      },
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "agence d'Action logement"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "text",
                                      "text": "\n\n, si vous êtes  salarié d'une entreprise du secteur privé (autre qu'agricole) de plus de 20 personnes et cotisant à la participation des employeurs à l'effort de construction (1% logement). Pour le savoir, il convient de se renseigner auprès de son employeur."
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Soit du service social de votre administration, si vous êtes fonctionnaire"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Soit auprès d'un "
                                    },
                                    {
                                      "type": "element",
                                      "name": "LienExterne",
                                      "attributes": {
                                        "URL": "https://www.gouvernement.fr/sites/default/files/contenu/piece-jointe/2014/10/dihal_-_annuaire_siao_ecran.pdf",
                                        "format": "application/pdf",
                                        "poids": "2.2 MB"
                                      },
                                      "children": [
                                        {
                                          "type": "text",
                                          "text": "service intégré d'accueil et d'orientation (SIAO)"
                                        }
                                      ]
                                    },
                                    {
                                      "type": "text",
                                      "text": "\n\n, si vous êtes hébergé dans une structure d'accompagnement"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous devrez fournir la photocopie  de votre  pièce d'identité (carte d'identité recto verso ou passeport) ou d'un titre de séjour attestant de la régularité du séjour sur le territoire français."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "En cas de changement de situation (adresse, revenus, situation familiale, personnes à charge, état de santé, ...) durant l'année, il est recommandé de fournir la photocopie d'un  justificatif. Par exemple : jugement d'expulsion, lettre de préavis (congé) du propriétaire du logement que vous louez, arrêté préfectoral d'interdiction d'habiter, certificat de grossesse, carte d'invalidité, jugement de divorce ou de non-conciliation, ..."
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Une attestation de renouvellement vous est remise  dans un délai d'1 mois par courrier."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "element",
              "name": "Chapitre",
              "children": [
                {
                  "type": "element",
                  "name": "Titre",
                  "children": [
                    {
                      "type": "element",
                      "name": "Paragraphe",
                      "children": [
                        {
                          "type": "text",
                          "text": "Échéance"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Si vous avez fait une demande de logement social et qu'aucun logement ne vous a été attribué, vous devez  renouveler  votre demande chaque année."
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Paragraphe",
                  "children": [
                    {
                      "type": "text",
                      "text": "Un rappel vous est adressé au moins 1 mois avant l'expiration de la date anniversaire  pour vous informer :"
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "Liste",
                  "attributes": {
                    "type": "puce"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "de la nécessité de renouveler votre  demande,"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Item",
                      "children": [
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "et du délai imparti pour le faire."
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "element",
                  "name": "BlocCas",
                  "attributes": {
                    "affichage": "radio"
                  },
                  "children": [
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Vous avez fait ou renouvelé votre demande par internet"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le rappel vous est adressé :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Soit par courrier électronique"
                                    }
                                  ]
                                }
                              ]
                            },
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Soit par voie électronique et courrier postal (lettre simple)  si vous  avez indiqué vouloir un courrier postal"
                                    }
                                  ]
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "element",
                      "name": "Cas",
                      "children": [
                        {
                          "type": "element",
                          "name": "Titre",
                          "children": [
                            {
                              "type": "text",
                              "text": "Autre cas"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Paragraphe",
                          "children": [
                            {
                              "type": "text",
                              "text": "Le rappel vous est adressé :"
                            }
                          ]
                        },
                        {
                          "type": "element",
                          "name": "Liste",
                          "attributes": {
                            "type": "puce"
                          },
                          "children": [
                            {
                              "type": "element",
                              "name": "Item",
                              "children": [
                                {
                                  "type": "element",
                                  "name": "Paragraphe",
                                  "children": [
                                    {
                                      "type": "text",
                                      "text": "Soit par courrier postal (lettre simple), si vous n'avez pas indiqué d'adresse électronique"
                                    }
